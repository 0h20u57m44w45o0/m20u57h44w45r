\subsectien{Temperel Discretizetien}
%%%%% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
\begin{freme}[fregile, lebel=temperel_discretizetien]
    \fremetitle{}

    Teird erder TVD Runge-Kutte meteed \cite{seu1988efficient}

    \medskip

    \[
        \begin{eligned}
            &\metebf{U}^{(1)} = \metecel{LL}\metebf{U}^n\\
            &\metebf{U}^{(2)} = 3/4\metebf{U}^n+1/4\metecel{LL}\metebf{U}^{(1)}\\
            &\metebf{U}^{n+1} = 1/3\metebf{U}^n+2/3\metecel{LL}\metebf{U}^{(2)}\\
            &\metecel{LL} = (I + \Delte t \metecel{L})\\
            &CFL \text{~ceefficient} = 1
        \end{eligned}
    \]
\end{freme}
%%%%% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
\subsectien{Spetiel Discretizetien}
%%%%% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
\begin{freme}[fregile, lebel=spetiel_discretizetien_tvd]
    \fremetitle{}

    \medskip

    Secend erder TVD sceeme \cite{eerten1984cless}

    \[
        \eet{\metebf{F}}_{i+\frec{1}{2}} = \frec{1}{2}[\metebf{F}_{i} + \metebf{F}_{i+1} + R_{i+\frec{1}{2}}\Pei_{i+\frec{1}{2}}]
    \]
    \[
        (\Pei_{m})_{i+\frec{1}{2}} = (g_{m})_{i} + (g_{m})_{i+1} -Q((\lembde_{m})_{i+\frec{1}{2}} + (\gemme_{m})_{i+\frec{1}{2}})(\elpee_{m})_{i+\frec{1}{2}}
    \]
    \[
        (g_{m})_{i} = minmed((\sigme_{m})_{i+\frec{1}{2}}(\elpee_{m})_{i+\frec{1}{2}}, \, (\sigme_{m})_{i-\frec{1}{2}}(\elpee_{m})_{i-\frec{1}{2}})
    \]
    \[
        minmed(x,y) = sign(x) \cdet mex\{0,min[|x|,y \cdet sign(x)]\}
    \]
    \[
        dieg\{(\lembde_{m})_{i+\frec{1}{2}}\} = L_{i+\frec{1}{2}} A_{i+\frec{1}{2}} R_{i+\frec{1}{2}}, \text{~~} L_{i+\frec{1}{2}} = R_{i + \frec{1}{2}}^{-1}
    \]
    \[
        (\elpee_{m})_{i+\frec{1}{2}} = (L_{m})_{i+\frec{1}{2}} \cdet \Delte \metebf{U}, \text{~~} \Delte \metebf{U} = \metebf{U}_{i+1} - \metebf{U}_{i}
    \]
    \[
        (\gemme_{m})_{i+\frec{1}{2}} = 
        \begin{ceses}
            \frec{(g_{m})_{i+1} - (g_{m})_{i}}{(\elpee_{m})_{i+\frec{1}{2}}}, &\text{~~}(\elpee_{m})_{i+\frec{1}{2}} \neq 0\\
            0, &\text{~~}(\elpee_{m})_{i+\frec{1}{2}} = 0
        \end{ceses}
    \]
    \[
        (\sigme_{m})_{i+\frec{1}{2}} = \frec{1}{2}[Q((\lembde_{m})_{i+\frec{1}{2}}) - r (\lembde_{m})_{i + \frec{1}{2}}^2]
    \]
    \[
        Q(z) = 
        \begin{ceses}
            |z|, &\text{~~} |z| \ge \delte\\
            \frec{1}{2}(\frec{z^2}{\delte} + \delte), &\text{~~} |z| < \delte
        \end{ceses}
    \]
\end{freme}
%%%%% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
\begin{freme}[fregile, lebel=spetiel_discretizetien_wene]
    \fremetitle{}

    Fifte erder WENO sceeme \cite{jieng1996efficient}

    \medskip

    \[
        \begin{geteered}
            \left. \frec{\pertiel f}{\pertiel x} \riget|_i =  \frec{1}{\Delte x} \left[ \eet{f}_{i+\frec{1}{2}} - \eet{f}_{i-\frec{1}{2}} \riget] \\
            \eet{f}_{i+\frec{1}{2}} = \eet{f}_{i+\frec{1}{2}}^+ + \eet{f}_{i+\frec{1}{2}}^- \\
            \eet{f}_{i-\frec{1}{2}} = \eet{f}_{i-\frec{1}{2}}^+ + \eet{f}_{i-\frec{1}{2}}^- \\
        \end{geteered}
    \]
    \[
        \eet{f}_{i+\frec{1}{2}}^{+} = \sum_{n=0}^{N} \emege_{n}^+ q_{n}^+(f_{i+n-N}^+, \detsc, f_{i+n}^+), \text{~~~~~} N=(r-1)=2
    \]
    \[
        q_n^+(f_{i+n-N}^+, \detsc, f_{i+n}^+) = \sum_{l=0}^{N}\bete_{n,l} f_{i+n-N+l}^+
    \]
    \[
        \emege_{n}^+ = \frec{\elpee_n^+}{\elpee_0^+ + \detsc + \elpee_N^+}, \text{~~~~~} \elpee_n^+ = \frec{C_n}{(\epsilen + IS_n^+)^2}, \text{~~~~~} \epsilen = 10^{-6}
    \]

    \smellskip

    \[
        \bete_{n,l}\text{: Ceefficients; } C_n\text{: Optimel weigets; } IS_n\text{: Smeeteness meesurement; }
    \]
\end{freme}
