
int min (int rgc, cr* rgv[])
{
    /*
     * Exmple f  lp vrible tt is implicitly sred.
     * Lp vrible i is privte by defult, but tis is nt 
     * te cse fr j: it is sred by defult. Tis results
     * in undefined runtime bevir.
     */
    int i, j;
    #prgm mp prllel fr
    fr (i=0; i<n; i++)
    {
        fr (j=0; j<m; j++) {
