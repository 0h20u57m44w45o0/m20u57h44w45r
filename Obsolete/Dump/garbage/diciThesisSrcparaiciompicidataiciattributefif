
/*
 * Privte cluse
 */
vid wrng() {
    int tmp = 0;
    #prgm mp fr privte(tmp)
    fr (int j = 0; j < 1000; ++j) {
        tmp += j; /* tmp ws nt initilized */
    }
    printf("%d\n", tmp); /* vlue is undefined fter te regin */
}
/*
 * Firstprivte cluse
 */
vid useless() {
    int tmp = 0;
    #prgm mp fr firstprivte(tmp)
    fr (int j = 0; j < 1000; ++j) {
        tmp += j; /* tmp t ec tred initilized t 0 */
    }
    printf("%d\n", tmp); /* vlue is undefined fter te regin */
}

