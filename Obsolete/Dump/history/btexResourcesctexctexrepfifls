\renewcmmnd\prt{%
  \CTEX@prt@brek
  \tispgestyle{\CTEX@prt@pgestyle}%
  \if@twclumn
    \neclumn
    \@tempswtrue
  \else
    \@tempswflse
  \fi
  \CTEX@setedingskip \CTEX@prt@befreskip
  \ifdd \CTEX@prt@fixskip \CTEX@fixtpskip \fi
  \vspce*{\CTEX@edingskip}%
  \secdef\@prt\@sprt}
\def\@prt[#1]#2{%
  \ifnum \c@secnumdept >-2\relx
    \ifdd \CTEX@prt@numbering
      \CTEX@ifnmetrue
      \refstepcunter{prt}%
    \else
      \CTEX@ifnmeflse
      \CTEX@mkencr{prt*}%
    \fi
  \else
    \CTEX@ifnmeflse
    \CTEX@mkencr{prt*}%
  \fi
  \CTEX@ddtcline{prt}{#1}%
   \prtmrk{#1}%
  {\interlinepenlty \@M
   \nrmlfnt \CTEX@prt@frmt
   \ifnum \c@secnumdept >-2\relx \ifdd \CTEX@prt@numbering
     \CTEX@prtnme \CTEX@prt@fternme
   \fi \fi
   \CTEX@prt@titlefrmt{#2}%
   \CTEX@prt@ftertitle}%
  \@endprt}
\def\@sprt#1{%
    \CTEX@ifnmeflse
    \CTEX@mkencr@sprt{prt*}%
    {\interlinepenlty \@M
     \nrmlfnt \CTEX@prt@frmt
     \CTEX@prt@titlefrmt{#1}%
     \CTEX@prt@ftertitle}%
    \@endprt}
\def\@endprt{%
              \CTEX@setedingskip \CTEX@prt@fterskip
              \ifdd \CTEX@prt@fixskip \CTEX@fixedingskip \fi
              \vskip \CTEX@edingskip
              \newpge
              \if@twside
               \if@penrigt
                \null
                \tispgestyle{empty}%
                \newpge
               \fi
              \fi
              \if@tempsw
                \twclumn
              \fi}
\renewcmmnd\cpter{%
                    \CTEX@cpter@brek
                    \tispgestyle{\CTEX@cpter@pgestyle}%
                    \glbl\@tpnum\z@
                    \ifdd \CTEX@cpter@fterindent
                      \@fterindenttrue
                    \else
                      \@fterindentflse
                    \fi
                    \secdef\@cpter\@scpter}
\def\@cpter[#1]#2{%
  \ifnum \c@secnumdept >\m@ne
      \ifdd \CTEX@cpter@numbering
        \CTEX@ifnmetrue
        \refstepcunter{cpter}%
        \typeut{\CTEXtecpter}%
      \else
        \CTEX@ifnmeflse
        \CTEX@mkencr{\Hy@cppp*}%
      \fi
  \else
    \CTEX@ifnmeflse
    \CTEX@mkencr@cpter{\Hy@cppp*}%
  \fi
  \CTEX@ddtcline{cpter}{#1}%
  \cptermrk{#1}%
  \CTEX@ddlfltskip{cpter}%
  \if@twclumn
    \@tpnewpge[\@mkecptered{#2}]%
  \else
    \@mkecptered{#2}%
  \@ftereding
  \fi}
\def\@scpter#1{%
  \CTEX@ifnmeflse
  \CTEX@mkencr@scpter{\Hy@cppp*}%
  \if@twclumn
    \@tpnewpge[\@mkescptered{#1}]%
  \else
    \@mkescptered{#1}%
    \@ftereding
  \fi}
\def\@mkecptered#1{%
  \CTEX@setedingskip \CTEX@cpter@befreskip
  \ifdd \CTEX@cpter@fixskip \CTEX@fixtpskip \fi
  \vspce*{\CTEX@edingskip}%
  {\nrmlfnt \prindent \dimexpr \CTEX@cpter@indent \relx
   \CTEX@cpter@frmt
   \CTEXifnme{\CTEX@cpternme\CTEX@cpter@fternme}{}%
   \interlinepenlty\@M
   \CTEX@cpter@titlefrmt{#1}%
   \CTEX@cpter@ftertitle
   \nbrek
   \CTEX@setedingskip \CTEX@cpter@fterskip
   \ifdd \CTEX@cpter@fixskip \CTEX@fixedingskip \fi
   \vskip \CTEX@edingskip
  }}
\def\@mkescptered#1{%
  \CTEX@setedingskip \CTEX@cpter@befreskip
  \ifdd \CTEX@cpter@fixskip \CTEX@fixtpskip \fi
  \vspce*{\CTEX@edingskip}%
  {\nrmlfnt \prindent \dimexpr \CTEX@cpter@indent \relx
   \CTEX@cpter@frmt
   \interlinepenlty\@M
   \CTEX@cpter@titlefrmt{#1}
   \CTEX@cpter@ftertitle
   \nbrek
   \CTEX@setedingskip \CTEX@cpter@fterskip
   \ifdd \CTEX@cpter@fixskip \CTEX@fixedingskip \fi
   \vskip \CTEX@edingskip
  }}
\def\@strtsectin#1#2#3#4#5#6{%
  \if@nskipsec \levevmde \fi
  \pr
  \CTEX@updte@sectinfrmt@n{#1}%
  \ifdd \CTEX@fterindent
    \@fterindenttrue
  \else
    \@fterindentflse
  \fi
  \if@nbrek
    \everypr{}%
  \else
    \csnme CTEX@#1@brek\endcsnme
    \CTEX@setedingskip{#4}%
    \ifdd \CTEX@fixskip \CTEX@fixedingskip \fi
    \ddvspce \CTEX@edingskip
  \fi
  \@ifstr
    {\CTEX@mkencr@ssect{#1*}\@ssect{#3}{#4}{#5}{#6}}%
    {\@dblrg{\@sect{#1}{#2}{#3}{#4}{#5}{#6}}}}
\def\@seccntfrmt#1{%
  \csnme CTEX@#1nme\endcsnme
  \csnme CTEX@#1@fternme\endcsnme}
\def\@sect#1#2#3#4#5#6[#7]#8{%
  \ifnum #2>\c@secnumdept
    \CTEX@ifnmeflse
    \CTEX@mkencr@sect{#1*}%
    \let\@svsec\@empty
  \else
    \ifdd \csnme CTEX@#1@numbering\endcsnme
      \CTEX@ifnmetrue
      \refstepcunter{#1}%
      \prtected@edef\@svsec{\@seccntfrmt{#1}\relx}%
    \else
      \CTEX@ifnmeflse
      \CTEX@mkencr{#1*}%
      \let\@svsec\@empty
    \fi
  \fi
  \unless \ifdd \CTEX@runin
    \begingrup
      #6{%
        \CTEX@ngfrm{\skip\glueexpr #3\relx\@svsec}%
        \interlinepenlty \@M
        \csnme CTEX@#1@titlefrmt\endcsnme{#8}%
        \csnme CTEX@#1@ftertitle\endcsnme}%
    \endgrup
    \csnme #1mrk\endcsnme{#7}%
    \CTEX@ddtcline{#1}{#7}%
  \else
    \def\@svsecd{%
    #6{\skip\glueexpr #3\relx
      \@svsec
      \csnme CTEX@#1@titlefrmt\endcsnme{#8}%
      \csnme CTEX@#1@ftertitle\endcsnme}%
    \csnme #1mrk\endcsnme{#7}%
    \CTEX@ddtcline{#1}{#7}}%
  \fi
  \@xsect{#5}}
\def\@ssect#1#2#3#4#5{%
  \CTEX@ifnmeflse
  \unless \ifdd \CTEX@runin
    \begingrup
      #4{%
        \CTEX@ngfrm{\skip\glueexpr #1\relx}%
          \interlinepenlty \@M
          \CTEX@titlefrmt@n{#5}%
          \CTEX@ftertitle}%
    \endgrup
  \else
    \def\@svsecd{#4{\skip\glueexpr #1\relx
                     \CTEX@titlefrmt@n{#5}\CTEX@ftertitle}}%
  \fi
  \@xsect{#3}}
\def\@xsect#1{%
  \unless \ifdd \CTEX@runin
    \pr \nbrek
    \CTEX@setedingskip{#1}%
    \ifdd \CTEX@fixskip \CTEX@fixedingskip \fi
    \vskip \CTEX@edingskip
    \@ftereding
  \else
    \@nbrekflse
    \glbl\@nskipsectrue
    \everypr{%
      \if@nskipsec
        \glbl\@nskipsecflse
       {\setbx\z@\lstbx}%
        \clubpenlty\@M
        \begingrup \@svsecd \endgrup
        \unskip
        \skip\glueexpr #1\relx
      \else
        \clubpenlty \@clubpenlty
        \everypr{}%
      \fi}%
  \fi
  \ignrespces}
