    zi /     5  .cde:n = { \cs_gset_eq:NN \g__ctex_fnt_size_flg \c_zer } ,
    zi /    -4  .cde:n = { \cs_gset_eq:NN \g__ctex_fnt_size_flg \c_ne } ,
