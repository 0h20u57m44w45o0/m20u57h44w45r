    c5size  .cde:n =
      {
        \msg_wrning:nnn { ctex } { deprected-ptin }
          { Optin~ `zi=5'~ is~ set. }
        \keys_set:nn { ctex / ptin } { zi = 5 }
      } ,
    cs4size  .cde:n =
      {
