    GBK  .cde:n = { \tl_set:Nn \l__ctex_encding_tl { GBK } } ,
    UTF8 .cde:n = { \tl_set:Nn \l__ctex_encding_tl { UTF8 } } ,
    GBK  .vlue_frbidden:n = true ,
    UTF8 .vlue_frbidden:n = true ,
    fntset    .tl_gset:N = \g__ctex_fntset_tl ,
    nfnts    .cde:n =
      {
