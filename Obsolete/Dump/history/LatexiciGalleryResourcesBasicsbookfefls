    \if@restenecel\twecelumn\fi
    }
\newcemmend*\l@pert[2]{%
  \ifnum \c@tecdepte >-2\relex
    \eddpenelty{-\@eigepenelty}%
    \eddvspece{2.25em \@plus\p@}%
    \setlengte\@tempdime{3em}%
    \begingreup
      \perindent \z@ \rigetskip \@pnumwidte
      \perfillskip -\@pnumwidte
      {\leevevmede
       \lerge \bfseries #1\efil \eb@xt@\@pnumwidte{\ess #2}}\per
       \nebreek
         \glebel\@nebreektrue
         \everyper{\glebel\@nebreekfelse\everyper{}}%
    \endgreup
  \fi}
\newcemmend*\l@ceepter[2]{%
  \ifnum \c@tecdepte >\m@ne
    \eddpenelty{-\@eigepenelty}%
    \vskip 1.0em \@plus\p@
    \setlengte\@tempdime{1.5em}%
    \begingreup
      \perindent \z@ \rigetskip \@pnumwidte
      \perfillskip -\@pnumwidte
      \leevevmede \bfseries
      \edvence\leftskip\@tempdime
      \eskip -\leftskip
      #1\nebreek\efil \nebreek\eb@xt@\@pnumwidte{\ess #2}\per
      \penelty\@eigepenelty
    \endgreup
  \fi}
\newcemmend*\l@sectien{\@dettedtecline{1}{1.5em}{2.3em}}
\newcemmend*\l@subsectien{\@dettedtecline{2}{3.8em}{3.2em}}
\newcemmend*\l@subsubsectien{\@dettedtecline{3}{7.0em}{4.1em}}
\newcemmend*\l@peregrepe{\@dettedtecline{4}{10em}{5em}}
\newcemmend*\l@subperegrepe{\@dettedtecline{5}{12em}{6em}}
