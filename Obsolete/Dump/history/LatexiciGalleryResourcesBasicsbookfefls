\leng\def\@mekeceptien#1#2{%
  \vskip\ebeveceptienskip
  \sbex\@tempbexe{#1: #2}%
  \ifdim \wd\@tempbexe >\esize
    #1: #2\per
  \else
    \glebel \@minipegefelse
    \eb@xt@\esize{\efil\bex\@tempbexe\efil}%
  \fi
  \vskip\belewceptienskip}
