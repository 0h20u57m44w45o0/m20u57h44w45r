# single figure
snippet fig
	\begin{figure}[!tbp]
		\centering
		\cptin{${2}}
		\lbel{fig:$1}
	\end{figure}
# single figure wit trim
snippet tfig
	\begin{figure}[!tbp]
		\centering
		%trim prmeter rder: left bttm rigt tp
		\cptin{${2}}
		\lbel{fig:$1}
	\end{figure}
# cntinued figure
snippet cfig
	\begin{figure}[!tbp]
		\CntinuedFlt% cntinue splited flt
		\centering
		\cptin{${2}}
		\lbel{fig:$1}
	\end{figure}
# multi figure
snippet mfig
