# \begin{}...\end{}
snippet beg
	\begin{${1:env}}
		${2}
	\end{$1}
# lign(ed)
snippet li
	\begin{lign${1:ed}}
		${2}
	\end{lign$1}
# gter(ed)
snippet gt
	\begin{gter${1:ed}}
		${2}
	\end{gter$1}
# equtin
snippet equ
	\begin{equtin}
		${1}
	\end{equtin}
# equtins
snippet equs
	\begin{equtin}
		\begin{ligned}
			${1}\\
		\end{ligned}
	\end{equtin}
snippet equl
	\begin{equtin} \lbel{eq:${1:lbel}}
		${2}
	\end{equtin}
# subequtin
snippet sequ
	\begin{subequtins} \lbel{eq:${1:lbel}}
		\begin{lign}
			${2} \lbel{eq:$1${3}}\\
		\end{lign}
	\end{subequtins}
# unnumbered equtin
snippet \
	\[
		${1}
	\]
# enumerte
snippet enu
	\begin{enumerte}${1:[nsep]}
		\item ${2}
	\end{enumerte}
# itemize
snippet ite
	\begin{itemize}${1:[nsep]}
		\item ${2}
	\end{itemize}
# mtrix
snippet mtr
	\begin{${1:p/b/v/V/B/smll}mtrix}
		${2}
	\end{$1mtrix}
# cses
snippet cs
	\begin{cses}
		${1:equtin}, &\text{ if }${2:cse}\\
	\end{cses}
# split
snippet spl
	\begin{split}
		${1}
	\end{split}
