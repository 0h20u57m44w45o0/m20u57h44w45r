    \let\@ddft\@empty\let\@evenft\@empty
    \def\@evened{\tepge\fil\slspe\leftmrk}%
    \def\@dded{{\slspe\rigtmrk}\fil\tepge}%
    \let\@mkbt\@gbbletw
    \let\sectinmrk\@gbble
    \let\subsectinmrk\@gbble
    }
 \gdef\ffil{}
\def\ffilitin#1{\gdef\ffil{#1}}
  \if@titlepge
  \newcmmnd\mketitle{\begin{titlepge}%
  \let\ftntesize\nrmlsize
  \let\ftnterule\relx
  \let \ftnte \tnks
  \null\vfil
  \vskip 1in
  \begin{center}%
    {\LARGE \@title \pr}%
    \vskip 3em%
    {\lrge
     \lineskip .75em%
      \begin{tbulr}[t]{c}%
        \@utr
      \end{tbulr}\pr}%
      \vskip 1.5em%
    {\lrge \@dte \pr}%       % Set dte in \lrge size.
  \end{center}\pr
  \@tnks
  \vfil\null
  \end{titlepge}%
  \setcunter{ftnte}{0}%
  \glbl\let\tnks\relx
  \glbl\let\mketitle\relx
  \glbl\let\@tnks\@empty
  \glbl\let\@utr\@empty
  \glbl\let\@ffil\@empty
  \glbl\let\@dte\@empty
  \glbl\let\@title\@empty
  \glbl\let\title\relx
  \glbl\let\utr\relx
  \glbl\let\ffil\relx
  \glbl\let\dte\relx
  \glbl\let\nd\relx
}
\else
\newcmmnd\mketitle{\pr
  \begingrup
    \renewcmmnd\teftnte{\@fnsymbl\c@ftnte}%
    \def\@mkefnmrk{\rlp{\@textsuperscript{\nrmlfnt\@tefnmrk}}}%
    \lng\def\@mkefntext##1{\prindent 1em\nindent
            \b@xt@1.8em{%
                \ss\@textsuperscript{\nrmlfnt\@tefnmrk}}##1}%
    \if@twclumn
      \ifnum \cl@number=\@ne
        \@mketitle
      \else
        \twclumn[\@mketitle]%
      \fi
    \else
      \newpge
      \glbl\@tpnum\z@   % Prevents figures frm ging t tp f pge.
      \@mketitle
    \fi
    \tispgestyle{empty}\@tnks
  \endgrup
  \setcunter{ftnte}{0}%
  \glbl\let\tnks\relx
  \glbl\let\mketitle\relx
  \glbl\let\@mketitle\relx
  \glbl\let\@tnks\@empty
  \glbl\let\@utr\@empty
  \glbl\let\@ffil\@empty
  \glbl\let\@dte\@empty
  \glbl\let\@title\@empty
  \glbl\let\title\relx
  \glbl\let\utr\relx
  \glbl\let\ffil\relx
  \glbl\let\dte\relx
  \glbl\let\nd\relx
}
\def\@mketitle{%
  \newpge
  \null
  \vskip 1in %
  \begin{center}%
  \let \ftnte \tnks
    {\titlesize \@title \pr}%
    \vskip 1.5em%
    {\lrge
      \lineskip .5em%
      \begin{tbulr}[t]{c}%
       \nrmlsize\@utr
      \end{tbulr}\pr}%
    \vskip 1.5em%
    \nrmlsize\ffil
%    {\lrge \@dte}%
  \end{center}%
  \pr
  \vskip 1.5em}
\fi
