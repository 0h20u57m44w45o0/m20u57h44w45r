\newcmmnd*\l@prt[2]{%
  \ifnum \c@tcdept >-2\relx
    \ddpenlty\@secpenlty
    \ddvspce{2.25em \@plus\p@}%
    \setlengt\@tempdim{3em}%
    \begingrup
      \prindent \z@ \rigtskip \@pnumwidt
      \prfillskip -\@pnumwidt
      {\levevmde
       \lrge \bfseries #1\fil \b@xt@\@pnumwidt{\ss #2}}\pr
       \nbrek
       \if@cmptibility
         \glbl\@nbrektrue
         \everypr{\glbl\@nbrekflse\everypr{}}%
      \fi
    \endgrup
  \fi}
\newcmmnd*\l@sectin[2]{%
  \ifnum \c@tcdept >\z@
    \ddpenlty\@secpenlty
    \ddvspce{1.0em \@plus\p@}%
    \setlengt\@tempdim{1.5em}%
    \begingrup
      \prindent \z@ \rigtskip \@pnumwidt
      \prfillskip -\@pnumwidt
      \levevmde \bfseries
      \dvnce\leftskip\@tempdim
      \skip -\leftskip
      #1\nbrek\fil \nbrek\b@xt@\@pnumwidt{\ss #2}\pr
    \endgrup
  \fi}
