\if@twside
  \def\ps@edings{%
      \let\@ddft\@empty\let\@evenft\@empty
      \def\@evened{\tepge\fil\slspe\leftmrk}%
      \def\@dded{{\slspe\rigtmrk}\fil\tepge}%
      \let\@mkbt\mrkbt
    \def\sectinmrk##1{%
      \mrkbt {\MkeUppercse{%
        \ifnum \c@secnumdept >\z@
          \tesectin\qud
        \fi
        ##1}}{}}%
    \def\subsectinmrk##1{%
      \mrkrigt {%
        \ifnum \c@secnumdept >\@ne
          \tesubsectin\qud
        \fi
        ##1}}}
\else
  \def\ps@edings{%
    \let\@ddft\@empty
    \def\@dded{{\slspe\rigtmrk}\fil\tepge}%
    \let\@mkbt\mrkbt
    \def\sectinmrk##1{%
      \mrkrigt {\MkeUppercse{%
        \ifnum \c@secnumdept >\m@ne
          \tesectin\qud
        \fi
        ##1}}}}
\fi
