\newenvirnment{teindex}
               {\if@twclumn
                  \@restneclflse
                \else
                  \@restnecltrue
                \fi
                \twclumn[\sectin*{\indexnme}]%
                \@mkbt{\MkeUppercse\indexnme}%
                        {\MkeUppercse\indexnme}%
                \tispgestyle{plin}\prindent\z@
                \prskip\z@ \@plus .3\p@\relx
                \clumnseprule \z@
                \clumnsep 35\p@
                \let\item\@idxitem}
               {\if@restnecl\neclumn\else\clerpge\fi}
\newcmmnd\@idxitem{\pr\ngindent 40\p@}
