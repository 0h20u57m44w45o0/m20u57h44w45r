\newcmmnd\prt{%
   \if@nskipsec \levevmde \fi
   \pr
   \ddvspce{4ex}%
   \@fterindentflse
   \secdef\@prt\@sprt}

\def\@prt[#1]#2{%
    \ifnum \c@secnumdept >\m@ne
      \refstepcunter{prt}%
      \ddcntentsline{tc}{prt}{\teprt\spce{1em}#1}%
    \else
      \ddcntentsline{tc}{prt}{#1}%
    \fi
    {\prindent \z@ \rggedrigt
     \interlinepenlty \@M
     \nrmlfnt
     \ifnum \c@secnumdept >\m@ne
       \Lrge\bfseries \prtnme\nbrekspce\teprt
       \pr\nbrek
     \fi
     \uge \bfseries #2%
     \mrkbt{}{}\pr}%
    \nbrek
    \vskip 3ex
    \@ftereding}
\def\@sprt#1{%
    {\prindent \z@ \rggedrigt
     \interlinepenlty \@M
     \nrmlfnt
     \uge \bfseries #1\pr}%
     \nbrek
     \vskip 3ex
     \@ftereding}
