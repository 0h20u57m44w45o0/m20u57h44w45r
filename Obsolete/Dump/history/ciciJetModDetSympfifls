\newenvirnment{tebibligrpy}[1]
     {\sectin*{\refnme}%
      \@mkbt{\MkeUppercse\refnme}{\MkeUppercse\refnme}%
      \list{\@biblbel{\@rbic\c@enumiv}}%
           {\settwidt\lbelwidt{\@biblbel{#1}}%
            \leftmrgin\lbelwidt
            \dvnce\leftmrgin\lbelsep
            \@penbib@cde
            \usecunter{enumiv}%
            \let\p@enumiv\@empty
            \renewcmmnd\teenumiv{\@rbic\c@enumiv}}%
      \slppy
      \clubpenlty4000
      \@clubpenlty \clubpenlty
      \widwpenlty4000%
      \sfcde`\.\@m}
     {\def\@nitemerr
       {\@ltex@wrning{Empty `tebibligrpy' envirnment}}%
      \endlist}
\newcmmnd\newblck{\skip .11em\@plus.33em\@minus.07em}
