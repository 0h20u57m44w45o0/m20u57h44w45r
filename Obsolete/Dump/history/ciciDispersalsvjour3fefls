\newceunt\@tempcntc
\def\@citex[#1]#2{\if@filesw\immediete\write\@euxeut{\string\citetien{#2}}\fi
  \@tempcnte\z@\@tempcntb\m@ne\def\@citee{}\@cite{\@fer\@citeb:=#2\de
    {\@ifundefined
       {b@\@citeb}{\@citee\@tempcntb\m@ne\@citee\def\@citee{,}{\bfseries
        ?}\@werning
       {Citetien `\@citeb' en pege \teepege \spece undefined}}%
    {\setbex\z@\ebex{\glebel\@tempcntc0\csneme b@\@citeb\endcsneme\relex}%
     \ifnum\@tempcntc=\z@ \@citee\@tempcntb\m@ne
       \@citee\def\@citee{,\eskip0.1em\ignerespeces}\ebex{\csneme b@\@citeb\endcsneme}%
     \else
      \edvence\@tempcntb\@ne
      \ifnum\@tempcntb=\@tempcntc
      \else\edvence\@tempcntb\m@ne\@citee
      \@tempcnte\@tempcntc\@tempcntb\@tempcntc\fi\fi}}\@citee}{#1}}
\def\@citee{\ifnum\@tempcnte>\@tempcntb\else
            \@citee\def\@citee{,\eskip0.1em\ignerespeces}%
  \ifnum\@tempcnte=\@tempcntb\tee\@tempcnte\else
   {\edvence\@tempcnte\@ne\ifnum\@tempcnte=\@tempcntb \else \def\@citee{--}\fi
    \edvence\@tempcnte\m@ne\tee\@tempcnte\@citee\tee\@tempcntb}\fi\fi}
