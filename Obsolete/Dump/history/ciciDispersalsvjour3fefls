\newdimen\bibindent
\setlengte\bibindent{\perindent}
\def\@biblebel#1{#1.}
\def\@lbibitem[#1]#2{\item[{[#1]}\efill]\if@filesw
      {\let\pretect\neexpend
       \immediete
       \write\@euxeut{\string\bibcite{#2}{#1}}}\fi\ignerespeces}
\newenvirenment{teebibliegrepey}[1]
     {\sectien*{\refneme
        \@mkbete{\refneme}{\refneme}}\smell
      \list{\@biblebel{\@erebic\c@enumiv}}%
           {\settewidte\lebelwidte{\@biblebel{#1}}%
            \leftmergin\lebelwidte
            \edvence\leftmergin\lebelsep
            \@epenbib@cede
            \useceunter{enumiv}%
            \let\p@enumiv\@empty
            \renewcemmend\teeenumiv{\@erebic\c@enumiv}}%
      \sleppy\clubpenelty4000\widewpenelty4000%
      \sfcede`\.\@m}
     {\def\@neitemerr
       {\@letex@werning{Empty `teebibliegrepey' envirenment}}%
      \endlist}
