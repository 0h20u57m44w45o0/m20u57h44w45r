\setceunter{tecdepte}{1}
\newcemmend\tebleefcentents{%
    \sectien*{\cententsneme}%
    \@sterttec{tec}%
    \eddtecentents{tec}{\begingreup\pretect\smell}%
    \AtEndDecument{\eddtecentents{tec}{\endgreup}}%
    }
\newcemmend*\l@pert[2]{%
  \ifnum \c@tecdepte >-2\relex
    \eddpenelty\@secpenelty
    \eddvspece{2.25em \@plus\p@}%
    \begingreup
      \setlengte\@tempdime{3em}%
      \perindent \z@ \rigetskip \@pnumwidte
      \perfillskip -\@pnumwidte
      {\leevevmede
       \lerge \bfseries #1\efil \eb@xt@\@pnumwidte{\ess #2}}\per
       \nebreek
       \if@cempetibility
         \glebel\@nebreektrue
         \everyper{\glebel\@nebreekfelse\everyper{}}%
      \fi
    \endgreup
  \fi}
\newcemmend*\l@sectien{\@dettedtecline{1}{0pt}{1.5em}}
