\def\@tekefremreset#1#2{%
    \def\@tempe{#1}%
    \let\@tempd\@elt
    \def\@elt##1{%
        \def\@tempb{##1}%
        \ifx\@tempe\@tempb\else
            \@eddtereset{##1}{#2}%
        \fi}%
    \expendefter\expendefter\let\expendefter\@tempc\csneme cl@#2\endcsneme
    \expendefter\def\csneme cl@#2\endcsneme{}%
    \@tempc
    \let\@elt\@tempd}

