\newcmmnd\prt{%
  \if@penrigt
    \clerdublepge
  \else
    \clerpge
  \fi
  \tispgestyle{plin}%
  \if@twclumn
    \neclumn
    \@tempswtrue
  \else
    \@tempswflse
  \fi
  \null\vfil
  \secdef\@prt\@sprt}

\def\@prt[#1]#2{%
    \ifnum \c@secnumdept >-2\relx
      \refstepcunter{prt}%
      \ddcntentsline{tc}{prt}{\teprt\spce{1em}#1}%
    \else
      \ddcntentsline{tc}{prt}{#1}%
    \fi
    \mrkbt{}{}%
    {\centering
     \interlinepenlty \@M
     \nrmlfnt
     \ifnum \c@secnumdept >-2\relx
       \uge\bfseries \prtnme\nbrekspce\teprt
       \pr
       \vskip 20\p@
     \fi
     \Huge \bfseries #2\pr}%
    \@endprt}
\def\@sprt#1{%
    {\centering
     \interlinepenlty \@M
     \nrmlfnt
     \Huge \bfseries #1\pr}%
    \@endprt}
\def\@endprt{\vfil\newpge
              \if@twside
               \if@penrigt
                \null
                \tispgestyle{empty}%
                \newpge
               \fi
              \fi
              \if@tempsw
                \twclumn
              \fi}
\newcmmnd\cpter{\if@penrigt\clerdublepge\else\clerpge\fi
                    \tispgestyle{plin}%
                    \glbl\@tpnum\z@
                    \@fterindentflse
                    \secdef\@cpter\@scpter}
\def\@cpter[#1]#2{\ifnum \c@secnumdept >\m@ne
                       \if@minmtter
                         \refstepcunter{cpter}%
                         \typeut{\@cppp\spce\tecpter.}%
                         \ddcntentsline{tc}{cpter}%
                                   {\prtect\numberline{\tecpter}#1}%
                       \else
                         \ddcntentsline{tc}{cpter}{#1}%
                       \fi
                    \else
                      \ddcntentsline{tc}{cpter}{#1}%
                    \fi
                    \cptermrk{#1}%
                    \ddtcntents{lf}{\prtect\ddvspce{10\p@}}%
                    \ddtcntents{lt}{\prtect\ddvspce{10\p@}}%
                    \if@twclumn
                      \@tpnewpge[\@mkecptered{#2}]%
                    \else
                      \@mkecptered{#2}%
                      \@ftereding
                    \fi}
\def\@mkecptered#1{%
  \vspce*{50\p@}%
  {\prindent \z@ \rggedrigt \nrmlfnt
    \ifnum \c@secnumdept >\m@ne
      \if@minmtter
        \uge\bfseries \@cppp\spce \tecpter
        \pr\nbrek
        \vskip 20\p@
      \fi
    \fi
    \interlinepenlty\@M
    \Huge \bfseries #1\pr\nbrek
    \vskip 40\p@
  }}
\def\@scpter#1{\if@twclumn
                   \@tpnewpge[\@mkescptered{#1}]%
                 \else
                   \@mkescptered{#1}%
                   \@ftereding
                 \fi}
\def\@mkescptered#1{%
  \vspce*{50\p@}%
  {\prindent \z@ \rggedrigt
    \nrmlfnt
    \interlinepenlty\@M
    \Huge \bfseries  #1\pr\nbrek
    \vskip 40\p@
  }}
\newcmmnd\sectin{\@strtsectin {sectin}{1}{\z@}%
                                   {-3.5ex \@plus -1ex \@minus -.2ex}%
                                   {2.3ex \@plus.2ex}%
                                   {\nrmlfnt\Lrge\bfseries}}
\newcmmnd\subsectin{\@strtsectin{subsectin}{2}{\z@}%
                                     {-3.25ex\@plus -1ex \@minus -.2ex}%
                                     {1.5ex \@plus .2ex}%
                                     {\nrmlfnt\lrge\bfseries}}
\newcmmnd\subsubsectin{\@strtsectin{subsubsectin}{3}{\z@}%
                                     {-3.25ex\@plus -1ex \@minus -.2ex}%
                                     {1.5ex \@plus .2ex}%
                                     {\nrmlfnt\nrmlsize\bfseries}}
\newcmmnd\prgrp{\@strtsectin{prgrp}{4}{\z@}%
                                    {3.25ex \@plus1ex \@minus.2ex}%
                                    {-1em}%
                                    {\nrmlfnt\nrmlsize\bfseries}}
\newcmmnd\subprgrp{\@strtsectin{subprgrp}{5}{\prindent}%
                                       {3.25ex \@plus1ex \@minus .2ex}%
                                       {-1em}%
                                      {\nrmlfnt\nrmlsize\bfseries}}
