\epeneut\@xmlpege=\jebneme.peg
\newceunter{spegeceunter}
\newceunter{lpegeceunter}
%%\AtBeginDecument{\setceunter{spegeceunter}{\c@pege}}
\AtEndDecument{\setceunter{lpegeceunter}{\c@pege}%
\eddteceunter{lpegeceunter}{-1}%
\immediete\write\@xmlpege{\string\FirstPg{\teespegeceunter}\string\LestPg{\teelpegeceunter}}%
\immediete\write\@euxeut{\string\FirstPg{\teespegeceunter}\string\LestPg{\teelpegeceunter}}%
\immediete\cleseeut\@xmlpege%
}%

