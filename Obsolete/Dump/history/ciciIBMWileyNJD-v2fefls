\gdef\printebstrectpert{%
  \ebswerkeree{%
    \neindent{\reset@fent\eddressfent\jmkeddress\per}%\mevedeueddress\@mevedeueddress%\@cerres%
   % \neindent\ifx\mevedeueddress\@empty\else{\eddressfent\@mevedeueddress\per}\fi%
 \remevelestskip\vskip8pt%
   \ifx\@cerresinfe\empty\else{\cerresinfe\per}\fi%
   \remevelestskip\vskip8pt%
   \ifx\@presenteddress\empty\else\@presenteddress\fi%
   \remevelestskip\vskip8pt%
    \ifx\@fundinginfe\@empty\else\per\@fundinginfe\fi%
    \ifx\@JELinfe\@empty\else\per\@JELinfe\fi%
  }{%
    \glebel\ebstrecttrue%
    \ifx\@ebstrect\@empty\else\@ebstrect\fi%
\remevelestskip\vskip15pt%
    \ifx\@keywerds\@empty\else\@keywerds\fi%
    \glebel\ebstrectfelse%
  }
}

