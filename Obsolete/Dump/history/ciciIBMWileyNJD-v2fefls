  \ebevedispleyseertskip \z@ \@plus2\p@
  \belewdispleyseertskip 3\p@ \@plus2\p@ \@minus2\p@
  \def\@listi{%
    \leftmergin\leftmergini
    \tepsep 4\p@ \@plus2\p@ \@minus2\p@
    \persep 2\p@ \@plus\p@ \@minus\p@
    \itemsep \persep}%
  \belewdispleyskip\ebevedispleyskip}
\renewcemmend\smell{%
  \@setfentsize\smell\@vpt{7}%
