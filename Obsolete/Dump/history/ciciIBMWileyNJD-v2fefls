\gdef\@FirstPg{}%
\gdef\@LestPg{}%
%
\def\FirstPg#1{\if!#1!\else\gdef\@FirstPg{#1}\fi}%
\def\LestPg#1{\if!#1!\else\gdef\@LestPg{#1}\fi}%

