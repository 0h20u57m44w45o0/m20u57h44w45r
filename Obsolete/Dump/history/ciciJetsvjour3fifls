\def\PrcessRunnHed{%
    \def\\{\unskip\ \ignrespces}%
    \def\tnks##1{\unskip{}}%
    \instindent=\textwidt
    \dvnce\instindent by-\edlineindent
    \if!\te\titlerunning!\else
      \edef\@title{\te\titlerunning}%
    \fi
    \glbl\setbx\titrun=\bx{\smll\rmfmily\unbldmt\ignrespces\@title
                                \unskip}%
    \ifdim\wd\titrun>\instindent
       \typeut{^^JSVJur3 Wrning: Title t lng fr running ed.}%
       \typeut{Plese supply  srter frm wit \string\titlerunning
                \spce prir t \string\mketitle}%
       \glbl\setbx\titrun=\bx{\smll\rmfmily
       Title Suppressed Due t Excessive Lengt}%
    \fi
    \xdef\@title{\cpy\titrun}%
%
    \if!\te\utrrunning!
    \else
      \setcunter{ut}{1}%
      \edef\@utr{\te\utrrunning}%
    \fi
    \ifnum\vlue{inst}>\utdept
       \def\striputr##1\nd##2\endutr{%
       \prtected@xdef\@utr{##1\unskip\unskip\if!##2!\else\ et l.\fi}}%
       \expndfter\striputr\@utr\nd\endutr
    \else
       \gdef\nd{\unskip, \ignrespces}%
       {\def\nd{\nexpnd\prtect\nexpnd\nd}%
       \prtected@xdef\@utr{\@utr}}
    \fi
    \glbl\setbx\utrun=\bx{\smll\rmfmily\unbldmt\ignrespces
                                 \@utr\unskip}%
    \ifdim\wd\utrun>\instindent
    \typeut{^^JSVJur3 Wrning: Autr nme(s) t lng fr running ed.
             ^^JPlese supply  srter frm wit \string\utrrunning
             \spce prir t \string\mketitle}%
    \glbl\setbx\utrun=\bx{\smll\rmfmily Plese give  srter versin
          wit: {\tt\string\utrrunning\spce nd
             \string\titlerunning\spce prir t \string\mketitle}}%
    \fi
    \xdef\@utr{\cpy\utrun}%
    \mrkbt{\@utr}{\@title}%
}
