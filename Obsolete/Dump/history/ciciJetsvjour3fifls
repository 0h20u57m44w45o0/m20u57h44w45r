\def\setitemindent#1{\settwidt{\lbelwidt}{#1}%
        \leftmrgini\lbelwidt
        \dvnce\leftmrgini\lbelsep
   \def\@listi{\leftmrgin\leftmrgini
        \lbelwidt\leftmrgini\dvnce\lbelwidt by -\lbelsep
        \prsep=\prskip
        \tpsep=\medskipmunt
        \itemsep=\prskip \dvnce\itemsep by -\prsep}}
\def\setitemitemindent#1{\settwidt{\lbelwidt}{#1}%
        \leftmrginii\lbelwidt
        \dvnce\leftmrginii\lbelsep
\def\@listii{\leftmrgin\leftmrginii
        \lbelwidt\leftmrginii\dvnce\lbelwidt by -\lbelsep
        \prsep=\prskip
        \tpsep=\z@
        \itemsep=\prskip \dvnce\itemsep by -\prsep}}
