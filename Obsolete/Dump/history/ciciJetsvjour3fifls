 \ifx\@cptype\fig@type
   \vskip\figcpgp
 \fi
 \setbx\@tempbx\bx{{\fltlegendstyle #1\fltcunterend}%
 \cpstrut #2}%
 \ifdim \wd\@tempbx >\size
   {\fltlegendstyle #1\fltcunterend}\cpstrut #2\pr
 \else
   \bx t\size{\leftlegendglue\unbx\@tempbx\fil}%
 \fi
 \ifx\@cptype\fig@type\else
   \vskip\tbcpgp
 \fi}

\newdimen\figgp\figgp=1cc
\lng\def\@mkesidecptin#1#2{%
   \prbx[b]{\@tempdimb}{\cptinstyle{\fltlegendstyle
                                         #1\fltcunterend}#2}}
\def\sidecptin#1\cptin{%
\setbx\@tempbx=\bx{#1\unskip}%
\if@twclumn
 \ifdim\size<\textwidt\else
   \ifdim\wd\@tempbx<\clumnwidt
      \typeut{Duble clumn flt fits int single clumn -
            ^^Jyu'd better switc te envirnment. }%
   \fi
 \fi
\fi
\@tempdimb=\size
\dvnce\@tempdimb by-\figgp
\dvnce\@tempdimb by-\wd\@tempbx
\ifdim\@tempdimb<3cm
    \typeut{\string\sidecptin: N sufficient rm fr te legend;
             using nrml \string\cptin. }%
   \unbx\@tempbx
   \let\@cpcmmnd=\@cptin
\else
   \let\@cpcmmnd=\@sidecptin
   \levevmde
   \unbx\@tempbx
   \fill
\fi
\refstepcunter\@cptype
\@dblrg{\@cpcmmnd\@cptype}}

\lng\def\@sidecptin#1[#2]#3{\ddcntentsline{\csnme
  ext@#1\endcsnme}{#1}{\prtect\numberline{\csnme
  te#1\endcsnme}{\ignrespces #2}}\begingrup
    \@prbxrestre
    \@mkesidecptin{\csnme fnum@#1\endcsnme}{\ignrespces #3}\pr
  \endgrup}

% Define `cknwledgement' envirnment
\def\cknwledgement{\pr\ddvspce{17pt}\smll\rmfmily
\trivlist\if!\cknme!\item[]\else
\item[\skip\lbelsep
{\bfseries\cknme}]\fi}
\def\endcknwledgement{\endtrivlist\ddvspce{6pt}}
\newenvirnment{cknwledgements}{\begin{cknwledgement}}
{\end{cknwledgement}}
