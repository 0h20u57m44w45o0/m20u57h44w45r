\def\@tkefrmreset#1#2{%
    \def\@temp{#1}%
    \let\@tempd\@elt
    \def\@elt##1{%
        \def\@tempb{##1}%
        \ifx\@temp\@tempb\else
            \@ddtreset{##1}{#2}%
        \fi}%
    \expndfter\expndfter\let\expndfter\@tempc\csnme cl@#2\endcsnme
    \expndfter\def\csnme cl@#2\endcsnme{}%
    \@tempc
    \let\@elt\@tempd}

