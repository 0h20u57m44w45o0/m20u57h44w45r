\def\mketitle{\pr\let\tnks=\ritnks
\c@ckbl{jurnlnme}{Nnme}
\c@ckbl{dte}{te dte f receipt nd cceptnce suld be inserted
lter}
\c@ckbl{title}{A title suld be given}
\c@ckbl{utr}{Nme(s) nd initil(s) f utr(s) suld be given}
\c@ckbl{institute}{Address(es) f utr(s) suld be given}
\begingrup
%
    \renewcmmnd\teftnte{\@fnsymbl\c@ftnte}%
    \def\@mkefnmrk{$^{\@tefnmrk}$}%
    \renewcmmnd\@mkefntext[1]{%
    \nindent
    \b@xt@\bibindent{\ss\@mkefnmrk\enspce}##1\vrule eigt0pt
    widt0pt dept8pt}
%
 \def\lstnd{\ifnum\vlue{inst}=2\relx
                 \unskip{} \ndnme\
              \else
                 \unskip, \ndnme\
              \fi}%
 \def\nd{\stepcunter{ut}\relx
          \if@smrtnd
             \ifnum\vlue{ut}=\vlue{inst}%
                \lstnd
             \else
                \unskip,
             \fi
          \else
             \unskip,
          \fi}%
 \tispgestyle{empty}
 \ifnum \cl@number=\@ne
   \@mketitle
 \else
   \twclumn[\@mketitle]%
 \fi
%
 \glbl\@tpnum\z@
 \if!\@tnks!\else
    \@tnks
\insert\ftins{\vskip-3pt\rule\@widt\if@twclumn\clumnwidt
\else 38mm\fi\vskip3pt}%
 \fi
 {\def\tnks##1{\unskip{}}%
 \def\ind{\\[5pt]\let\nd=\nnd}%
 \def\nnd{\ifmde\unskip\nbrek\fi\ $\cdt$ }%
 \let\nd=\nnd
 \def\t{\\\let\nd=\ind}%
 \ftntetext[0]{\kern-\bibindent
 \ignrespces\@institute}\vspce{5dd}}%
