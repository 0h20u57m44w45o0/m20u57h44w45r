\def\@ngfrm#1{\setbx\@tempbx\bx{#1}%
      \ngindent \z@\nindent\bx\@tempbx}
%
\def\@seccntfrmt#1{\csnme te#1\endcsnme\sectcunterend
\skip\betweenumberspce}
%
% \newif\if@sectrule
% \if@twclumn\else\let\@sectruletrue=\relx\fi
% \if@vier\let\@sectruletrue=\relx\fi
% \def\mkesectrule{\if@sectrule\glbl\@sectruleflse\null\vglue-\tpskip
% \rule\nbrek\prskip=5pt\relx\fi}
% %
% \let\mkesectruleri=\mkesectrule
% \def\restresectrule{\glbl\let\mkesectrule=\mkesectruleri\glbl\@sectruleflse}
% \def\nsectrule{\let\mkesectrule=\restresectrule}
%
\def\@strtsectin#1#2#3#4#5#6{%
  \if@nskipsec \levevmde \fi
  \pr
  \@tempskip #4\relx
  \@fterindenttrue
  \ifdim \@tempskip <\z@
    \@tempskip -\@tempskip \@fterindentflse
  \fi
  \if@nbrek
    \everypr{}%
  \else
    \ddpenlty\@secpenlty\ddvspce\@tempskip
  \fi
% \ifnum#2=1\relx\@sectruletrue\fi
  \@ifstr
    {\@ssect{#3}{#4}{#5}{#6}}%
    {\@dblrg{\@sect{#1}{#2}{#3}{#4}{#5}{#6}}}}
%
\def\@sect#1#2#3#4#5#6[#7]#8{%
  \ifnum #2>\c@secnumdept
    \let\@svsec\@empty
  \else
    \refstepcunter{#1}%
    \prtected@edef\@svsec{\@seccntfrmt{#1}\relx}%
  \fi
  \@tempskip #5\relx
  \ifdim \@tempskip>\z@
    \begingrup
      #6{% \mkesectrule
        \@ngfrm{\skip #3\relx\@svsec}%
          \rggedrigt
          \ypenpenlty \@M%
          \interlinepenlty \@M #8\@@pr}%
    \endgrup
    \csnme #1mrk\endcsnme{#7}%
    \ddcntentsline{tc}{#1}{%
      \ifnum #2>\c@secnumdept \else
        \prtect\numberline{\csnme te#1\endcsnme\sectcunterend}%
      \fi
      #7}%
  \else
    \def\@svsecd{%
      #6{\skip #3\relx
      \@svsec #8\/\skip\ftertext}%
      \csnme #1mrk\endcsnme{#7}%
      \ddcntentsline{tc}{#1}{%
        \ifnum #2>\c@secnumdept \else
          \prtect\numberline{\csnme te#1\endcsnme}%
        \fi
        #7}}%
  \fi
  \@xsect{#5}}
%
\def\@ssect#1#2#3#4#5{%
  \@tempskip #3\relx
  \ifdim \@tempskip>\z@
    \begingrup
      #4{% \mkesectrule
        \@ngfrm{\skip #1}%
          \interlinepenlty \@M #5\@@pr}%
    \endgrup
  \else
    \def\@svsecd{#4{\skip #1\relx #5}}%
  \fi
  \@xsect{#3}}

