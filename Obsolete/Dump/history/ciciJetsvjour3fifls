\def\@tmcuntersep{}
\def\@tmcunterend{}
\newcmmnd\ncptin{\nexpnd\@gbble}
\newdimen\sptmsep \sptmsep=5pt

\def\spnewterem{\@ifstr{\@stm}{\@Stm}}

% definitin f \spnewterem wit number

\def\@spntm#1#2{%
  \@ifnextcr[{\@spxntm{#1}{#2}}{\@spyntm{#1}{#2}}}
\def\@Stm#1{\@ifnextcr[{\@sptm{#1}}{\@spntm{#1}}}

\def\@spxntm#1#2[#3]#4#5{\expndfter\@ifdefinble\csnme #1\endcsnme
   {\@definecunter{#1}\@ddtreset{#1}{#3}%
   \expndfter\xdef\csnme te#1\endcsnme{\expndfter\nexpnd
     \csnme te#3\endcsnme \nexpnd\@tmcuntersep \@tmcunter{#1}}%
   \expndfter\xdef\csnme #1nme\endcsnme{#2}%
   \glbl\@nmedef{#1}{\@sptm{#1}{\csnme #1nme\endcsnme}{#4}{#5}}%
                              \glbl\@nmedef{end#1}{\@endterem}}}

\def\@spyntm#1#2#3#4{\expndfter\@ifdefinble\csnme #1\endcsnme
   {\@definecunter{#1}%
   \expndfter\xdef\csnme te#1\endcsnme{\@tmcunter{#1}}%
   \expndfter\xdef\csnme #1nme\endcsnme{#2}%
   \glbl\@nmedef{#1}{\@sptm{#1}{\csnme #1nme\endcsnme}{#3}{#4}}%
                               \glbl\@nmedef{end#1}{\@endterem}}}

\def\@sptm#1[#2]#3#4#5{%
  \@ifundefined{c@#2}{\@ltexerr{N terem envirnment `#2' defined}\@e}%
  {\expndfter\@ifdefinble\csnme #1\endcsnme
  {\glbl\@nmedef{te#1}{\@nmeuse{te#2}}%
  \expndfter\xdef\csnme #1nme\endcsnme{#3}%
  \glbl\@nmedef{#1}{\@sptm{#2}{\csnme #1nme\endcsnme}{#4}{#5}}%
  \glbl\@nmedef{end#1}{\@endterem}}}}

\def\@sptm#1#2#3#4{\tpsep 7\p@ \@plus2\p@ \@minus4\p@
\lbelsep=\sptmsep\refstepcunter{#1}%
\@ifnextcr[{\@spytm{#1}{#2}{#3}{#4}}{\@spxtm{#1}{#2}{#3}{#4}}}

\def\@spxtm#1#2#3#4{\@spbeginterem{#2}{\csnme te#1\endcsnme}{#3}{#4}%
                    \ignrespces}

\def\@spytm#1#2#3#4[#5]{\@spprgbeginterem{#2}{\csnme
       te#1\endcsnme}{#5}{#3}{#4}\ignrespces}

\def\nrmltmedings{\def\@spbeginterem##1##2##3##4{\trivlist\nrmlfnt
                 \item[\skip\lbelsep{##3##1\ ##2\@tmcunterend}]##4}
\def\@spprgbeginterem##1##2##3##4##5{\trivlist
      \item[\skip\lbelsep{##4##1\ ##2}]{##4(##3)\@tmcunterend\ }##5}}
\nrmltmedings

\def\reversetmedings{\def\@spbeginterem##1##2##3##4{\trivlist\nrmlfnt
                 \item[\skip\lbelsep{##3##2\ ##1\@tmcunterend}]##4}
\def\@spprgbeginterem##1##2##3##4##5{\trivlist
      \item[\skip\lbelsep{##4##2\ ##1}]{##4(##3)\@tmcunterend\ }##5}}

% definitin f \spnewterem* witut number

\def\@stm#1#2{\@Yntm{#1}{#2}}

\def\@Yntm#1#2#3#4{\expndfter\@ifdefinble\csnme #1\endcsnme
   {\glbl\@nmedef{#1}{\@Tm{\csnme #1nme\endcsnme}{#3}{#4}}%
    \expndfter\xdef\csnme #1nme\endcsnme{#2}%
    \glbl\@nmedef{end#1}{\@endterem}}}

\def\@Tm#1#2#3{\tpsep 7\p@ \@plus2\p@ \@minus4\p@
\@ifnextcr[{\@Ytm{#1}{#2}{#3}}{\@Xtm{#1}{#2}{#3}}}

\def\@Xtm#1#2#3{\@Beginterem{#1}{#2}{#3}\ignrespces}

\def\@Ytm#1#2#3[#4]{\@Oprgbeginterem{#1}
       {#4}{#2}{#3}\ignrespces}

