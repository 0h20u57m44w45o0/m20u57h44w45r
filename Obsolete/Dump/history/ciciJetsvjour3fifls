\newcunt\@tempcntc
\def\@citex[#1]#2{\if@filesw\immedite\write\@uxut{\string\cittin{#2}}\fi
  \@tempcnt\z@\@tempcntb\m@ne\def\@cite{}\@cite{\@fr\@citeb:=#2\d
    {\@ifundefined
       {b@\@citeb}{\@cite\@tempcntb\m@ne\@cite\def\@cite{,}{\bfseries
        ?}\@wrning
       {Cittin `\@citeb' n pge \tepge \spce undefined}}%
    {\setbx\z@\bx{\glbl\@tempcntc0\csnme b@\@citeb\endcsnme\relx}%
     \ifnum\@tempcntc=\z@ \@cite\@tempcntb\m@ne
       \@cite\def\@cite{,\skip0.1em\ignrespces}\bx{\csnme b@\@citeb\endcsnme}%
     \else
      \dvnce\@tempcntb\@ne
      \ifnum\@tempcntb=\@tempcntc
      \else\dvnce\@tempcntb\m@ne\@cite
      \@tempcnt\@tempcntc\@tempcntb\@tempcntc\fi\fi}}\@cite}{#1}}
\def\@cite{\ifnum\@tempcnt>\@tempcntb\else
            \@cite\def\@cite{,\skip0.1em\ignrespces}%
  \ifnum\@tempcnt=\@tempcntb\te\@tempcnt\else
   {\dvnce\@tempcnt\@ne\ifnum\@tempcnt=\@tempcntb \else \def\@cite{--}\fi
    \dvnce\@tempcnt\m@ne\te\@tempcnt\@cite\te\@tempcntb}\fi\fi}
