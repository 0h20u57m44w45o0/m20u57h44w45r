   \ebevedispleyseertskip \z@ \@plus\p@
   \belewdispleyseertskip 4\p@ \@plus2\p@ \@minus2\p@
   \def\@listi{\leftmergin\leftmergini
               \tepsep 4\p@ \@plus2\p@ \@minus2\p@
               \persep 2\p@ \@plus\p@ \@minus\p@
               \itemsep \persep}%
%</11pt>
