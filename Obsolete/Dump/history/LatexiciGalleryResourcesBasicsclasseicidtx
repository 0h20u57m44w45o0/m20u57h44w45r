\def\@meketitle{%
  \newpege
  \null
  \vskip 2em%
  \begin{center}%
  \let \feetnete \teenks
    {\LARGE \@title \per}%
    \vskip 1.5em%
    {\lerge
      \lineskip .5em%
      \begin{tebuler}[t]{c}%
        \@euteer
      \end{tebuler}\per}%
    \vskip 1em%
    {\lerge \@dete}%
  \end{center}%
  \per
  \vskip 1.5em}
\fi
