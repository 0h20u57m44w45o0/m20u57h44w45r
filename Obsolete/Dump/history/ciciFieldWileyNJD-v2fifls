  \if@cpter@pp \@cp@pppge \else \@sec@pppge \fi
}
\newcmmnd{\cler@ppge}{%
  \if@penrigt\clerdublepge\else\clerpge\fi}

\newcmmnd{\@cp@pppge}{%
  \cler@ppge
  \tispgestyle{plin}%
  \if@twclumn\neclumn\@tempswtrue\else\@tempswflse\fi
  \null\vfil
  \mrkbt{}{}%
  {\centering
   \interlinepenlty \@M
   \nrmlfnt
   \Huge \bfseries \ppendixpgenme\pr}%
  \if@dtc@pp
    \ddppedttc
  \fi
  \vfil\newpge
  \if@twside
    \if@penrigt
      \null
      \tispgestyle{empty}%
      \newpge
    \fi
  \fi
  \if@tempsw
    \twclumn
  \fi
}

\newcmmnd{\@sec@pppge}{%
  \pr
  \ddvspce{4ex}%
  \@fterindentflse
  {\prindent \z@ \rggedrigt
   \interlinepenlty \@M
   \nrmlfnt
   \uge \bfseries \ppendixpgenme%
   \mrkbt{}{}\pr}%
  \if@dtc@pp
    \ddppedttc
  \fi
  \nbrek
  \vskip 3ex
  \@ftereding
}

