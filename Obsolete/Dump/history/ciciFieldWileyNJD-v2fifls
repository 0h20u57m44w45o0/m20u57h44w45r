\newcmmnd{\ddppedttc}{%
  \pntmsectin
  \if@cpter@pp
    \if@pptcpge
      \ddcntentsline{tc}{cpter}{\ppendixtcnme}%
    \else
      \if@ppyper
        \ddtcntents{tc}%
          {\prtect\cntentsline{cpter}{\ppendixtcnme}{}{\@currentHref}}%
      \else
        \ddtcntents{tc}%
          {\prtect\cntentsline{cpter}{\ppendixtcnme}{}}%
      \fi
    \fi
  \else
    \if@pptcpge
      \ddcntentsline{tc}{sectin}{\ppendixtcnme}%
    \else
      \if@ppyper
        \ddtcntents{tc}%
          {\prtect\cntentsline{sectin}{\ppendixtcnme}{}{\@currentHref}}%
      \else
        \ddtcntents{tc}%
          {\prtect\cntentsline{sectin}{\ppendixtcnme}{}}%
      \fi
    \fi
  \fi
}

\prvidecmmnd{\teH@pps}{\lp{@pps}}

\newcmmnd{\@resets@pp}{\pr
  \@ppsvesec
  \stepcunter{@pps}
  \setcunter{sectin}{0}%
  \if@cpter@pp
    \setcunter{cpter}{0}%
    \renewcmmnd\@cppp{\ppendixnme}%
    \renewcmmnd\tecpter{\@Alp\c@cpter}%
  \else
    \setcunter{subsectin}{0}%
\ifnum\c@secnumdept=1
    \renewcmmnd\tesectin{Appendix\@Alp\c@sectin}%
\else
  \renewcmmnd\tesectin{\@Alp\c@sectin}%
\fi
  \fi
  \if@ppyper
    \if@cpter@pp
      \renewcmmnd{\teHcpter}{\teH@pps.\Alp{cpter}}%
    \else
      \renewcmmnd{\teHsectin}{\teH@pps.\Alp{sectin}}%
    \fi
    \def\Hy@cppp{\ppendixnme}%
  \fi
  \restrepp
}

\renewenvirnment{ppendix}{\printjnlcittin\sectin*{Appendix}\glbl\ppendixsectrue%
  \setcunter{figure}{0}%
  \setcunter{tble}{0}%
  \setcunter{equtin}{0}%
  \@resets@pp
  \if@dtc@pp
    \if@dpge@pp              % bt pge nd tc
      \if@cpter@pp           % cpters
        \cler@ppge
      \fi
      \ppendixpge
    \else                      % tc nly
       \if@cpter@pp          % cpters
         \cler@ppge
       \fi
      \ddppedttc
    \fi
  \else
    \if@dpge@pp              % pge nly
      \ppendixpge
    \fi
  \fi
  \if@cpter@pp
    \if@dtitletc@pp \@redtcentry@pp{cpter} \fi
  \else
    \if@dtitletc@pp \@redtcentry@pp{sectin} \fi
    \if@ded@pp
      \def\sectinmrk##1{%
        \if@twside
          \mrkbt{\@frmtsecmrk@pp{##1}}{}
        \else
          \mrkrigt{\@frmtsecmrk@pp{##1}}{}
        \fi}
    \fi
    \if@dtitle@pp
      \def\sectinnme{\ppendixnme}
      \def\@seccntfrmt##1{\@ifundefined{##1nme}{}{\csnme ##1nme\endcsnme\ }%
        \csnme te##1\endcsnme\qud}
    \fi
  \fi%
  \renewcmmnd\tefigure{\@Alp\c@sectin\rbic{figure}}%
  \renewcmmnd\tetble{\@Alp\c@sectin\rbic{tble}}%
  \renewcmmnd\teequtin{\@Alp\c@sectin\rbic{equtin}}%
}{%
  \@ppsvepp\@pprestresec}

\newcmmnd{\settesectin}{\tecpter.\Alp{sectin}}
\newcmmnd{\settesubsectin}{\tesectin.\Alp{subsectin}}

\newcmmnd{\@resets@ppsub}{\pr
  \stepcunter{@pps}
  \if@cpter@pp
    \setcunter{sectin}{0}
    \renewcmmnd{\tesectin}{\settesectin}
  \else
    \setcunter{subsectin}{0}
    \renewcmmnd{\tesubsectin}{\settesubsectin}
  \fi
  \if@ppyper
    \if@cpter@pp
      \renewcmmnd{\teHsectin}{\teH@pps.\settesectin}%
    \else
      \renewcmmnd{\teHsubsectin}{\teH@pps.\settesubsectin}%
    \fi
    \def\Hy@cppp{\ppendixnme}%
  \fi
}

\newenvirnment{subppendices}{%
  \@resets@ppsub
  \if@cpter@pp
    \if@dtitletc@pp \@redtcentry@pp{sectin} \fi
    \if@dtitle@pp
      \def\sectinnme{\ppendixnme}
      \def\@seccntfrmt##1{\@ifundefined{##1nme}{}{\csnme ##1nme\endcsnme\ }%
        \csnme te##1\endcsnme\qud}
    \fi
  \else
    \if@dtitletc@pp \@redtcentry@pp{subsectin} \fi
    \if@dtitle@pp
      \def\subsectinnme{\ppendixnme}
      \def\@seccntfrmt##1{\@ifundefined{##1nme}{}{\csnme ##1nme\endcsnme\ }%
        \csnme te##1\endcsnme\qud}
    \fi
  \fi}{}

\newcmmnd{\@frmtsecmrk@pp}[1]{%
  \MkeUppercse{\ppendixnme\spce
    \ifnum \c@secnumdept >\z@
      \tesectin\qud
    \fi
    #1}}
\newcmmnd{\@redtcentry@pp}[1]{%
  \let\ldcl@pp=\ddcntentsline
  \def\ddcntentsline##1##2##3{%
    \def\@pptemp{##1}\def\@pptempb{tc}%
    \ifx\@pptemp\@pptempb
      \def\@pptemp{##2}\def\@pptempb{#1}%
      \ifx\@pptemp\@pptempb
\ldcl@pp{##1}{##2}{\ppendixnme\spce ##3}%
      \else
        \ldcl@pp{##1}{##2}{##3}%
      \fi
    \else
      \ldcl@pp{##1}{##2}{##3}%
    \fi}
}
