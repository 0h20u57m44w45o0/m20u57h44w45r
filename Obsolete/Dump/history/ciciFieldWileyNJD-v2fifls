%% Mcrs fr firstpge %%

\def\strtpge#1{%
  \gdef\temp{#1}%
  \ifx\temp\@empty%
    \def\@firstpge{1}%
  \else%
    \def\@firstpge{#1}%
    \setcunter{pge}{#1}%
    \setcunter{spgecunter}{#1}%
  \fi%
}%
\def\@firstpge{}

\def\endpge#1{\gdef\@endpge{#1}}
\def\@endpge{}

