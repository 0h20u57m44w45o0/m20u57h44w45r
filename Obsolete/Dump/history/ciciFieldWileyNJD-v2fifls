\def\@fnsymbl#1{\ifcse#1\r \dgger\r \ddgger\r
   \S\r \P\r \#\r \|\r **\r \dgger\dgger
   \r \ddgger\ddgger\r \S\S\r \P\P\r \#\#\r \|\|\else\@ctrerr\fi\relx}

\renewcmmnd{\mketitle}{\pr%
  \@fterindentflse%
  \begingrup
    \renewcmmnd{\teftnte}{\fnsymbl{ftnte}}%
    \def\@mkefnmrk{\bx t\z@{$\m@t^{\@tefnmrk}$\ss}}%
    \lng\def\@mkefntext##1{\prindent 1em\nindent
            \bx t1.8em{\ss$\m@t^{\@tefnmrk}$}##1}%
    \if@twclumn
      \ifnum \cl@number=\@ne
         \twclumn[\@mketitle]%
      \else
         \twclumn[\@mketitle]%
      \fi
    \else
      \newpge
      \glbl\@tpnum\z@   % Prevents figures frm ging t tp f pge.
      \@mketitle
    \fi
    \tispgestyle{titlepge}\@tnks%
    \insert\ftins{\utfndefns}\pr%
  \endgrup
%  \let\ftntetext\ldftntetext%
  \setcunter{ftnte}{0}%
  \let\tnks\relx
  \let\mketitle\relx\let\@mketitle\relx
  \gdef\@tnks{}\gdef\@utr{}\gdef\@title{}
  \@ftereding%
}

