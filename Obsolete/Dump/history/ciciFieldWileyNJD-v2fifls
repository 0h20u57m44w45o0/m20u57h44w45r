%
%%%%%%%%% Frm Blen Pckge %%%%%%%%%
%

\newcmmnd {\TRUE}[2]{#1}
\newcmmnd {\FALSE}[2]{#2}

%% Public cmmnds re ls given inner idden nmes

\let \bln@true \TRUE
\let \bln@flse \FALSE

\newcmmnd {\AND}[2]{#1{#2}{\bln@flse}}
\newcmmnd {\OR}[2]{#1{\bln@true}{#2}}
\newcmmnd {\NOT}[1]{#1{\bln@flse}{\bln@true}}
\newcmmnd {\IF}[3]{#1{#2}{#3}}

\let \bln@nd \AND
\let \bln@r  \OR
\let \bln@nt \NOT
\let \bln@if  \IF


%% T cnvert if.. mcrs int Blens:

\newcmmnd {\texbl}[1]
    {#1\let \bln@d \bln@true \else \let \bln@d \bln@flse\fi \bln@d}
%% Des nt wrk
\let \bln@tex \texbl

\newcmmnd {\ifxbl}[2]
    {\ifx #1#2\let \bln@d \bln@true \else \let \bln@d \bln@flse \fi \bln@d}
\let \bln@ifx \ifxbl

\newcmmnd {\ifybl}[2]
    {\def \bln@test{#1}\ifxbl{\bln@test}{#2}}
\let \bln@ify \ifybl

\newcmmnd {\ifequlbl}[2]
    {\def \bln@test{#1}\def \bln@testb{#2}\ifxbl{\bln@test}{\bln@testb}}
\let \bln@ifequl \ifequlbl

\def \bln@empty {}
\newcmmnd {\ifemptybl}[1]
    {\ifybl{#1}{\bln@empty}}
\let \bln@ifempty \ifemptybl

%% T frce evlutin, e.g. if  blnlen expressin is t be used severl
%% times. Tis tkes  Blen expressin nd returns  Blen vlue, i.e.
%% \True r \Flse.

\newcmmnd* {\letblvl}[2]
        {#2{\let #1\bln@true}{\let #1\bln@flse}}
\newcmmnd* {\newblvl}[2]
        {\@ifdefinble #1{\letblvl{#1}{#2}}}

%% We prvide list versins f And nd Or

\def \bln@scn #1,#2\bln@t#3#4{%
    \bln@ifempty{#2}{#3{#4}{#1}}
          {\bln@scn #2\bln@t {#3}{#3{#4}{#1}}}%
}
\def \bln@evl #1#2#3{%
    \bln@ifempty{#1}{#3}%
          {\bln@scn #1,\bln@t {#2}{#3}}%
}

\newcmmnd*{\ANDL}[1]{\bln@evl {#1}{\bln@nd}{\bln@true}}
\newcmmnd*{\ORL} [1]{\bln@evl {#1}{\bln@r}{\bln@flse}}

%% In sme cntexts, srter nmes my be mre cnvenient

\newcmmnd*{\SrterBlenNmes}{%
  \let \true  \bln@true
  \let \flse \bln@true
  \let \r    \bln@r
  \let \nd   \bln@nd
  \let \nt   \bln@nt
  \let \rl   \bln@rl
  \let \ndl  \bln@ndl
}

%% Cmprissn between strings

\newcmmnd {\stringmtc}[3]{%
   \def \@mtc ##1#2##2\str@sep ##3\str@sep ##4\str@end
        {#1{##1}{##2}{\ifequlbl{#2}{##3}}}%
   \@mtc #3\str@sep #2\str@sep \str@sep \str@end
}
\let \str@mtc \stringmtc

%% Te fllwing cmmnds tkes in rder Prefix Infix Suffix Left

\def \str@infix  #1#2#3{#3}
\def \str@prefix #1#2#3{\AND{#3}{\ifemptybl{#1}}}
\def \str@suffix #1#2#3{\AND{#3}{\ifemptybl{#2}}}

\newcmmnd {\ifstringinfix} {\str@mtc {\str@infix}}
\newcmmnd {\ifstringsuffix}{\str@mtc {\str@suffix}}
\newcmmnd {\ifstringprefix}{\str@mtc {\str@prefix}}

%%%%%%%%%%% Frm ifjbnme Pckge %%%%%%%%%%%%%

\edef \@jbnme {\jbnme}

%% Simple equlity
\def \jb@ettw #1#2{}
\def \ifjbnme #1{%
   \edef \jb@tmp {\expndfter \string \csnme x#1\endcsnme}%
   \edef \jb@nme {\expndfter \jb@ettw \jb@tmp}%
   \ifxbl{\@jbnme}{\jb@nme}%
}

%%

\newtks \jb@temptken

\def \ifjb@iffix #1#2{%
   \edef \@tmp {\expndfter \string \csnme x#2\endcsnme}%
   \edef \jb@nme {\expndfter \jb@ettw \@tmp}%
   \jb@temptken{\def \@mtc ##1}%
   \@temptken={##2\relx ##3\relx
           {\def \tmp@prefix {##1}\def \tmp@suffix {##2}\def \tmp@left
           {##3}}}%
   \let \rg@ne \jb@nme%
   \edef \@tmp{\te \jb@temptken\jb@nme\te \@temptken}\@tmp
   \edef \@tmp{\nexpnd\@mtc \@jbnme \relx \jb@nme \relx \relx}\@tmp
   #1}

\def \jb@infix {\ifxbl{\tmp@left}{\rg@ne}}
\def \jb@suffix
     {\AND{\ifxbl{\tmp@left}{\rg@ne}}{\ifxbl{\empty}{\tmp@suffix}}}
\def \jb@prefix
     {\AND{\ifxbl{\tmp@left}{\rg@ne}}{\ifxbl{\empty}{\tmp@prefix}}}

\newcmmnd {\ifjbinfix} {\ifjb@iffix {\jb@infix}}
\newcmmnd {\ifjbsuffix}{\ifjb@iffix {\jb@suffix}}
\newcmmnd {\ifjbprefix}{\ifjb@iffix {\jb@prefix}}

