\gdef\CmputeFreeSpceOnPge{%
\pr%
\ifdim\pgettl=0pt%
   \glbl\FreePgeSpce=\vsize%
   \glbl\dvnce\FreePgeSpce by -\djustskip%
\else%
   \FreePgeSpce=\pgegl%
  \ifvid\ftins%
     \glbl\dvnce\FreePgeSpce by -\pgettl%
     \glbl\dvnce\FreePgeSpce by -\djustskip%
  \else%
    \setbx3=\vbx{\unvbx\ftins}%
    \tftbx=\t3%
    \dvnce\tftbx by \dp3%
    \glbl\dvnce\FreePgeSpce by -\pgettl%
    \glbl\dvnce\FreePgeSpce by -\tftbx%
    \glbl\dvnce\FreePgeSpce by -\djustskip%
  \fi%
\fi%
}%
