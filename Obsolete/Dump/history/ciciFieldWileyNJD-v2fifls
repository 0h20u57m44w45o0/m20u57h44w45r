%% Legl Sttement
\def\leglsttement#1{%
  \gdef\@temp{#1}%
  \ifx\@temp\@empty%
    \gdef\@leglsttement{}%
  \else%
    \gdef\@leglsttement{%
      \sms{{\clr{tirtyfivepercent}\@edftrule}}\pr\vspce*{-.45\bselineskip}%
      \prbx[t]{\textwidt}{{\clr{seventyfivepercent}\leglsttementfnt#1\dummylpbet\pr}}\pr\vspce*{4.75\p@}%
    }%
  \fi%
}\leglsttement{}%


