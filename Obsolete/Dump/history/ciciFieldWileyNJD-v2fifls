%% Mcrs fr disply equtins %%

\let\SplitEq\relx%

\setlengt\rryclsep{2\p@}%
\setlengt\tbclsep{6\p@}%
\setlengt\rryrulewidt{.4\p@}%
\setlengt\dublerulesep{4\p@}%
\setlengt\tbbingsep{\lbelsep}%

\def\eqnrry{%%\initilset%%%intrduced n 31Oct15%
   \stepcunter{equtin}%
   \def\@currentlbel{\p@equtin\teequtin}%
   \glbl\@eqnswtrue
   \m@t
   \glbl\@eqcnt\z@
   \tbskip\@centering%
   \let\\\@eqncr
   $$\everycr{}\lign t\displywidt\bgrup
       \skip\@centering$\displystyle\tbskip\z@skip{##}$\@eqnsel% fr centered equtins
%%     \skip\z@skip\fil$\displystyle\tbskip\z@skip{##}$\@eqnsel% fr left ligned equtins
      &\glbl\@eqcnt\@ne\skip \tw@\rryclsep \fil${##}$\fil
      &\glbl\@eqcnt\tw@ \skip \tw@\rryclsep
         $\displystyle{##}$\fil\tbskip\@centering
      &\glbl\@eqcnt\tr@@ \b@xt@\z@\bgrup\ss##\egrup
       \tbskip\z@skip
      \cr
}
\def\endeqnrry{%
      \@@eqncr
      \egrup
      \glbl\dvnce\c@equtin\m@ne
   $$\@ignretrue
}
