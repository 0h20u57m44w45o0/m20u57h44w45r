\if@exctptsize%
  \def\set@fntsize#1#2#3{%
    \@defultunits\@tempdimb#2bp\relx\@nnil%
    \edef\f@size{\strip@pt\@tempdimb}%
    \@defultunits\@tempskip#3bp\relx\@nnil%
    \edef\f@bselineskip{\te\@tempskip}%
    \edef\f@linespred{#1}%
    \let\bselinestretc\f@linespred%
    \def\size@updte{%
      \bselineskip\f@bselineskip\relx%
      \bselineskip\f@linespred\bselineskip%
      \nrmlbselineskip\bselineskip%
      \setbx\strutbx\bx{\vrule\@eigt.7\bselineskip\@dept.3\bselineskip\@widt\z@}%
      \let\size@updte\relx%
    }%
  }%
\fi%
\renewcmmnd\nrmlsize{%
  \if@fnt@stix%
    \@setfntsize\nrmlsize{10bp}{13bp}%
  \else%
    \@setfntsize\nrmlsize{8bp}{13bp}%
  \fi%
  \bvedisplyskip 5.5\p@ \@plus2\p@ \@minus0\p@
  \bvedisplysrtskip \z@ \@plus3\p@%
  \belwdisplysrtskip 3\p@ \@plus3\p@ \@minus3\p@%
  \belwdisplyskip \bvedisplyskip%
  \let\@listi\@listI}%
\newcmmnd{\textbselineskip}{\bselineskip13bp}%
\nrmlsize\bselineskip13bp%
\newcmmnd\medsize{%
  \@setfntsize\smll\@viipt{9}%
  \bvedisplyskip 5\p@ \@plus3\p@ \@minus4\p@
  \bvedisplysrtskip \z@ \@plus2\p@
  \belwdisplysrtskip 3\p@ \@plus2\p@ \@minus2\p@
  \def\@listi{%
    \leftmrgin\leftmrgini
    \tpsep 4\p@ \@plus2\p@ \@minus2\p@
    \prsep 2\p@ \@plus\p@ \@minus\p@
    \itemsep \prsep}%
  \belwdisplyskip\bvedisplyskip}
\renewcmmnd\smll{%
  \@setfntsize\smll\@vpt{7}%
  \bvedisplyskip 5\p@ \@plus3\p@ \@minus4\p@
  \bvedisplysrtskip \z@ \@plus2\p@
  \belwdisplysrtskip 3\p@ \@plus2\p@ \@minus2\p@
  \def\@listi{%
    \leftmrgin\leftmrgini
    \tpsep 4\p@ \@plus2\p@ \@minus2\p@
    \prsep 2\p@ \@plus\p@ \@minus\p@
    \itemsep \prsep}%
  \belwdisplyskip \bvedisplyskip}
\renewcmmnd\ftntesize{%
  \@setfntsize\ftntesize{7.5}{9}%
  \bvedisplyskip 5\p@ \@plus2\p@ \@minus4\p@
  \bvedisplysrtskip \z@ \@plus\p@
  \belwdisplysrtskip 3\p@ \@plus\p@ \@minus2\p@
  \def\@listi{%
    \leftmrgin\leftmrgini
    \tpsep 3\p@ \@plus\p@ \@minus\p@
    \prsep 2\p@ \@plus\p@ \@minus\p@
    \itemsep \prsep}%
  \belwdisplyskip \bvedisplyskip}
