\def\@utrmrk{}% 
\def\utrmrk#1{%
\mrkbt{{{#1}}}{{{#1}}}\gdef\@utrmrk{\def\etl{\textit{ET AL.}}\let\AQ\AHAQ#1}\gdef\@utrmrkfrcite{\undefined\def\etl{\textit{et l.}}#1}}%
\def\etl{\textit{et l.}}%

\newskip\rverticlsep\rverticlsep11.5\p@%
\def\ddpge@edings@def{\spce*{\rverticlsep}\rvertbr\pgenumberlign{\ss\tepgedef}}%
\def\evenpge@edings@def{\pgenumberlign{\tepgedef\ss}\rvertbr\spce*{\rverticlsep}}%

\def\leftmrkdef{{\runningedfnt\@utrmrk}}%
\def\rigtmrkdef{{\runningedfnt\@utrmrk}}%
%
\def\edftrule#1{\gdef\@edftrule{#1}}\edftrule{{\rule{\textwidt}{.5\p@}}}%
%
\def\ed@edings@def#1{\vbx{%
  \strut\pr\vspce*{-5pt}%
\vspce*{2\p@}%\fi%
  \@edftrule\pr%\vspce*{-8.25\p@}%
\vspce*{-11\p@}%\fi%
  \bx t \textwidt{#1\dummylpbet}\pr\vspce*{\p@}%
  }%
}%
%

\def\dded@edings@inf{\ed@edings@def{\rigtmrkdef\fill\ddpge@edings@def}}%
\def\evened@edings@inf{\ed@edings@def{\evenpge@edings@def\fill\leftmrkdef}}%

\def\ps@edings{%
\let\@ddft\@empty\let\@evenft\@empty%
  \def\@dded{\dded@edings@inf}%
  \def\@evened{\evened@edings@inf}%
\let\@mkbt\mrkbt%
