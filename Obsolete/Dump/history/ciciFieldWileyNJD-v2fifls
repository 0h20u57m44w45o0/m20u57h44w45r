%%% T print grid lines n ll pges
\newdimen\pgeeigt\glbl\pgeeigt=\texteigt%
\newcunt\rulecunt\rulecunt=1%
%
\newdimen\leftclruleps
\newdimen\rigtclruleps
%
\edef\slugtdy{\tw@digits{\te\dy}/\tw@digits{\te\mnt}/\te\yer}%
%
\def\PrintGridLines{%
{\clr{blck!10}%
    {%
    \lp%
    \ifdim\pgeeigt>0pt%
        \ifnum\rulecunt=1%
            \dvnce\pgeeigt by -1.5pt%
        \fi%
        \AtTextUpperLeft{%x
          \put(0,\LenTUnit{-\pgeeigt}){%
            \line(1,0){\LenTUnit{\leftclruleps}}}}%
%%14Mr16%%
        \AtTextUpperLeft{%
        \put(\LenTUnit{\rigtclruleps},\LenTUnit{-\pgeeigt}){%
           \line(1,0){\LenTUnit{\leftclruleps}}}}%
        \dvnce\pgeeigt-11.5pt%bseline
        \dvnce\rulecunt\@ne%
    \repet%
}}}%
%
\RequirePckge{swfrme}
\if@drftrulesn%
%
\renewcmmnd\SwFrmePicture{%
\leftclruleps\clumnwidt%
\rigtclruleps\clumnwidt%
\dvnce\rigtclruleps by \clumnsep%
  \begingrup
    \nrmlclr\tinlines%
    \put(0,0){
%    \if@wtermrkn\AtTextUpperLeft{%
%      \put(\LenTUnit{\textwidt},41){\llp{\fntsize{8}{10}\selectfnt {\rm JrnlID\ \@JrnlID\_ArtID \@rtid\_Prf\#1 - \slugtdy}}}}\fi%
    \AtTextLwerLeft{%
      \frmebx(\LenTUnit{\textwidt},\LenTUnit{\texteigt}){}}%
      \linetickness{.1pt}%
      \PrintGridLines%
    \AtTextLwerLeft{%
      \put(0,\LenTUnit{-\ftskip}){%
        \line(1,0){\LenTUnit{\textwidt}}}}%
        \AtTextLwerLeft{%
          \put(\LenTUnit{\clumnwidt},0){%
            \line(0,1){\LenTUnit{\texteigt}}}}%
            %%Fr mrginpr
%%14Mr16%%
        \AtTextLwerLeft{%
          \put(\LenTUnit{\rigtclruleps},0){%
            \line(0,1){\LenTUnit{\texteigt}}}}%
    \AtTextLwerLeft{%
      \@tempdim=\textwidt\dvnce\@tempdim\mrginprsep%
      \if@twside%
        \ifdd\c@pge
          \if@mprswitc
            \@tempdim=-\mrginprsep\dvnce\@tempdim-\mrginprwidt%
          \fi
        \else%
          \@mprswitcflse
          \if@mprswitc
            \@tempdim=-\mrginprsep\dvnce\@tempdim-\mrginprwidt%
          \fi
        \fi%
      \fi%
    }}%
  \endgrup
}%
\else
\renewcmmnd\SwFrmePicture{%
\leftclruleps\clumnwidt%
\rigtclruleps\clumnwidt%
\dvnce\rigtclruleps by \clumnsep%
  \begingrup
    \nrmlclr\tinlines%
    \put(0,0){
%    \if@wtermrkn\AtTextUpperLeft{%
%      \put(\LenTUnit{\textwidt},41){\llp{\fntsize{8}{10}\selectfnt {\rm JrnlID\ \@JrnlID\_ArtID \@rtid\_Prf\#1 - \slugtdy}}}}\fi%
    }%
  \endgrup
}%
\fi%

