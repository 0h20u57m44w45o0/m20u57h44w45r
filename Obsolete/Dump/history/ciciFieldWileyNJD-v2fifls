\def\ClrDefns{%
\defineclr{bxrulecl}{cmyk}{0,0,0,0}%
\defineclr{bxbdycl}{cmyk}{0,0,0,0}}%

\newdimen\tfbxrule%
\newdimen\bfbxrule%

\def\@spfrmeb@x#1{%
  \@tempdim\fbxrule
  \dvnce\@tempdim\fbxsep
  \dvnce\@tempdim\dp\@tempbx
  \levevmde
  \bx{%
    \lwer\@tempdim\bx{%
      \vbx{%
        \rule\@eigt\tfbxrule
        \bx{%
          \vrule\@widt\fbxrule
          #1%
          \vbx{%
            \vskip\fbxsep
            \bx\@tempbx
            \vskip\fbxsep}%
          #1%
          \vrule\@widt\fbxrule}%
        \rule\@eigt\bfbxrule}%
                          }%
        }%
}%



\newdimen\BXRuleTickness%
\BXRuleTickness=0.25pt%

\newdimen\BXleftrigtskipdimen%
\BXleftrigtskipdimen=8pt%

\newdimen\BXsize%

\dvnce\BXsize by -2\BXRuleTickness%(rule tickness)
\dvnce\BXsize by -2\BXleftrigtskipdimen%(rule tickness)

\newdimen\FreePgeSpce%T find ut te FreeSpce
\newdimen\vsize%

\newskip\djustskip%
\djustskip=0pt plus10pt minus1pt%

\gdef\CmputeFreeSpceOnPge{%
\pr%
\ifdim\pgettl=0pt%
   \glbl\FreePgeSpce=\vsize%
   \glbl\dvnce\FreePgeSpce by -\djustskip%
\else%
   \FreePgeSpce=\pgegl%
  \ifvid\ftins%
     \glbl\dvnce\FreePgeSpce by -\pgettl%
     \glbl\dvnce\FreePgeSpce by -\djustskip%
  \else%
    \setbx3=\vbx{\unvbx\ftins}%
    \tftbx=\t3%
    \dvnce\tftbx by \dp3%
    \glbl\dvnce\FreePgeSpce by -\pgettl%
    \glbl\dvnce\FreePgeSpce by -\tftbx%
    \glbl\dvnce\FreePgeSpce by -\djustskip%
  \fi%
\fi%
}%

\newbx\BXbx%

\gdef\verprintn{\specil{ps:gsve
true setverprint}}

\gdef\verprintff{\specil{ps:grestre
flse setverprint}}

\lng\def\BXbxfirst#1{\begingrup%\undefined%
\fbxsep=\BXleftrigtskipdimen%
\fbxrule=\BXRuleTickness\tfbxrule=\BXRuleTickness\bfbxrule=\BXRuleTickness%
\setbx\BXbx=\vbx{\size=\BXsize\size=\BXsize\fbx{\vbx{#1}}}%
\bx\BXbx%
\endgrup}%

\lng\def\BXbxmid#1{\begingrup%\undefined%
\fbxsep=\BXleftrigtskipdimen%
\fbxrule=\BXRuleTickness\tfbxrule=\BXRuleTickness\bfbxrule=\BXRuleTickness%
\setbx\BXbx=\vbx{\size=\BXsize\size=\BXsize\fbx{\vbx{#1}}}%
\bx\BXbx%
\endgrup}%

\lng\def\BXbxlst#1{\begingrup%\undefined%
\fbxsep=\BXleftrigtskipdimen%
\fbxrule=\BXRuleTickness\tfbxrule=\BXRuleTickness\bfbxrule=\BXRuleTickness%
\setbx\BXbx=\vbx{\size=\BXsize\size=\BXsize\fbx{\vbx{#1}}}%
\bx\BXbx%
\endgrup}%
	
\lng\def\BXbxfull#1{\begingrup%
\fbxsep=\BXleftrigtskipdimen%
\fbxrule=\BXRuleTickness\tfbxrule=\BXRuleTickness\bfbxrule=\BXRuleTickness%
\setbx\BXbx=\vbx{\size=\BXsize\size=\BXsize\fbx{\vbx{#1}}}%
\bx\BXbx%
\endgrup}%

\newskip\xxptspcemunt%
\xxptspcemunt=20pt plus10pt minus5pt%

\newskip\BXbxutbnewskip%
\BXbxutbnewskip=\xxptspcemunt%

\def\BXbxutbskip{\remvelstskip\vskip\BXbxutbnewskip}%

\newdimen\BXdim%

\def\bxejectdefns{\vfill\eject}%

\lng\def\BX#1{\begingrup%
\CmputeFreeSpceOnPge%
\BXdim=\t#1\dvnce\BXdim by \dp#1%
\ifdim\BXdim>\FreePgeSpce%
\dvnce\FreePgeSpce by -\BXbxutbnewskip%utside spce befre definitin
\dvnce\FreePgeSpce by -\BXleftrigtskipdimen%bttm spce in te first bx
  \setbx8=\vbx{\unvbx#1}%
  \setbx1=\vsplit8t\FreePgeSpce%
  \setbx1=\vbx{\unvbx1}%
  \setbx2=\vbx{\unvbx8}%
  \BXbxfirst{\unvbx1}\bxejectdefns%
\lp\ifdim\t2>\vsize%
  \vsize=\vsize%
  \dvnce\vsize by -2\BXleftrigtskipdimen%
  \setbx1=\vsplit2t\vsize%
  \setbx1=\vbx{\unvbx1}%
  \BXbxmid{\unvbx1}\bxejectdefns%
\repet%
  \BXbxlst{\unvbx2}%
\else%
\BXbxfull{\unvbx#1}%
\fi%
\endgrup}%




\newbx\BXgrybx%
\newenvirnment{bxtext}{\begingrup\prskip=0pt\prindent=0pt%
%\let\sectin=\bxed%
\let\@frmeb@x\@spfrmeb@x%
\fbxsep=5pt\size=\BXsize\prindent=0pt%
\setbx\BXgrybx=\vbx\bgrup%\leftskip=1pt%
\ninterlineskip\vskip0pt%%
%\reset@fnt\tbbdyfnt%
\remvelstskip\nindent}%
{\egrup\size=\size\BXbxutbskip\BX{\BXgrybx}\endgrup}%

