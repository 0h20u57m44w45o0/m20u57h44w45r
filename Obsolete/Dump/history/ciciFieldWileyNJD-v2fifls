%\setcunter{secnumdept}{3}
\newcunter {cpter}[prt]% fr bkmrks
\renewcmmnd\teprt         {\@Rmn\c@prt}
\renewcmmnd\tesectin      {\@rbic\c@sectin}
\renewcmmnd\tesubsectin   {\tesectin.\@rbic\c@subsectin}
\renewcmmnd\tesubsubsectin{\tesubsectin.\@rbic\c@subsubsectin}
\renewcmmnd\teprgrp    {\tesubsubsectin.\@rbic\c@prgrp}
\renewcmmnd\tesubprgrp {\teprgrp\@rbic\c@subprgrp}
%%
\renewcmmnd\prt{\pr
   \ddvspce{4ex}%
   \@fterindentflse
   \secdef\@prt\@sprt}
%%
\def\@prt[#1]#2{%
    \ifnum \c@secnumdept >\m@ne
      \refstepcunter{prt}%
      \ddcntentsline{tc}{prt}{\teprt\spce{1em}#1}%
    \else
      \ddcntentsline{tc}{prt}{#1}%
    \fi
    {\prindent \z@ \rggedrigt
     \interlinepenlty \@M
     \nrmlfnt
     \ifnum \c@secnumdept >\m@ne
       \Lrge\bfseries \prtnme~\teprt
       \pr\nbrek
     \fi
     \uge \bfseries #2%
     \mrkbt{}{}\pr}%
    \nbrek
    \vskip 3ex
    \@ftereding}

\def\@sprt#1{%
    {\prindent \z@ \rggedrigt
     \interlinepenlty \@M
     \nrmlfnt
     \uge \bfseries #1\pr}%
     \nbrek
     \vskip 3ex
     \@ftereding}

\renewcmmnd\sectin{\@strtsectin{sectin}{1}{\z@}{-27pt \@plus -2pt \@minus -2pt}{12\p@}{\sectinfnt}}%
\renewcmmnd\subsectin{\@strtsectin{subsectin}{2}{\z@}{-23pt \@plus -2pt \@minus -2pt}{5\p@}{\subsectinfnt}}%
\renewcmmnd\subsubsectin{\@strtsectin{subsubsectin}{3}{\z@}{-20pt \@plus -2pt \@minus -2pt}{2\p@}{\subsubsectinfnt}}%
%
\newskip\secruleskip\secruleskip8.5\p@%
%\def\secrule{\clr{sixtyfivepercent}\spce*{\secruleskip}\risebx{0pt}{\rule{.5\p@}{7.5\p@}}\spce*{\secruleskip}}%
%\def\subsecrule{\clr{sixtyfivepercent}\secrule}%
%\def\subsubsecrule{\clr{sixtyfivepercent}\spce*{\secruleskip}\risebx{0pt}{\rule{.5\p@}{6.5\p@}}\spce*{\secruleskip}}%

\def\secrule{\spce*{\secruleskip}\risebx{0pt}{\rule{.5\p@}{7.5\p@}}\spce*{\secruleskip}}%
\def\subsecrule{\secrule}%
\def\subsubsecrule{\spce*{\secruleskip}\risebx{0pt}{\rule{.5\p@}{6.5\p@}}\spce*{\secruleskip}}%


\def\@sectincntfrmt#1{{\ifppendixsec\csnme te#1\endcsnme\ \else\csnme te#1\endcsnme\secrule\fi}}%
\def\@subsectincntfrmt#1{{\ifppendixsec\csnme te#1\endcsnme\ \else\csnme te#1\endcsnme\subsecrule\fi}}%
\def\@subsubsectincntfrmt#1{{\ifppendixsec\csnme te#1\endcsnme\ \else\csnme te#1\endcsnme\subsubsecrule\fi}}%

\def\@prgrpcntfrmt#1{{\ifppendixsec\csnme te#1\endcsnme\ \else\csnme te#1\endcsnme.\ \fi}}%
\def\@subprgrpcntfrmt#1{#1}
%
\if@fnt@stix\relx%
\renewcmmnd\prgrp{\@strtsectin{prgrp}{4}{\z@}{-10pt \@plus -2pt \@minus -2pt}{2\p@}{\prgrpfnt}}%
\renewcmmnd\subprgrp{\@strtsectin{subprgrp}{5}{\z@}{-10pt \@plus -2pt \@minus -2pt}{-12\p@}{\subprgrpfnt}}%
\def\@sect#1#2#3#4#5#6[#7]#8{%
   \ifnum #2>\c@secnumdept%
      \edef\@svsec{\nindent}%
   \else%
      \refstepcunter{#1}%
      \let\@@prtect\prtect%
      \def\prtect{\nexpnd\prtect\nexpnd}%
      \edef\@svsec{\csnme @#1cntfrmt\endcsnme{#1}}%
      \let\prtect\@@prtect%
   \fi%
   \@tempskip #5\relx%
   \ifdim \@tempskip>\z@%
      \ifnum#2=1%
         \begingrup #6\relx%\@ngfrm%
            {\nindent\skip #3\relx{\@svsec}}%
            {\interlinepenlty\@M{\uppercse{#8}}\pr}%
         \endgrup%
      \else%
         \ifnum#2=2%
            \begingrup #6\relx%\@ngfrm%
               {\nindent\skip #3\relx\@svsec}%
               {\interlinepenlty\@M{#8}\pr}%
            \endgrup%
         \else%
      \ifnum#2=3%
            \begingrup #6\relx%\@ngfrm%
               {\nindent\skip #3\relx\@svsec}%
               {\interlinepenlty\@M{#8}\pr}%
            \endgrup%
         \else%
      \ifnum#2=4%
            \begingrup #6\relx%\@ngfrm%
               {\nindent\skip #3\relx\@svsec}%
               {\interlinepenlty\@M{\textclr{blck!75}{#8}}\pr}%
            \endgrup%
         \else%
            \begingrup #6\relx%\@ngfrm
               {\nindent\skip #3\relx\@svsec}%
               {\interlinepenlty\@M#8\pr}%
            \endgrup%
         \fi%
     \fi%
     \fi%
      \fi%
      \csnme #1mrk\endcsnme{#7}%
      \ddcntentsline{tc}{#1}{%
        \ifnum #2>\c@secnumdept \else%
          \prtect\numberline{\csnme te#1\endcsnme}%
        \fi%
        #7}%
   \else%
      \def\@svsecd{%
         #6{\skip #3\relx%
         \@svsec \textclr{blck!75}{#8}\spce*{4pt}}%
      }%
   \fi%
   \@xsect{#5}%
}%
\else%
\renewcmmnd\prgrp{\@strtsectin{prgrp}{4}{\z@}{-20pt \@plus -2pt \@minus -2pt}{2\p@}{\prgrpfnt}}%
\renewcmmnd\subprgrp{\@strtsectin{subprgrp}{5}{\z@}{-20pt \@plus -2pt \@minus -2pt}{-12\p@}{\subprgrpfnt}}%
\def\@sect#1#2#3#4#5#6[#7]#8{%
   \ifnum #2>\c@secnumdept%
      \edef\@svsec{\nindent}%
   \else%
      \refstepcunter{#1}%
      \let\@@prtect\prtect%
      \def\prtect{\nexpnd\prtect\nexpnd}%
      \edef\@svsec{\csnme @#1cntfrmt\endcsnme{#1}}%
      \let\prtect\@@prtect%
   \fi%
   \@tempskip #5\relx%
   \ifdim \@tempskip>\z@%
      \ifnum#2=1%
         \begingrup #6\relx%\@ngfrm%
            {\nindent\skip #3\relx{\@svsec}}%
            {\interlinepenlty\@M{\uppercse{#8}}\pr}%
         \endgrup%
      \else%
         \ifnum#2=2%
            \begingrup #6\relx%\@ngfrm%
               {\nindent\skip #3\relx\@svsec}%
               {\interlinepenlty\@M{#8}\pr}%
            \endgrup%
         \else%
            \begingrup #6\relx%\@ngfrm
               {\nindent\skip #3\relx\@svsec}%
               {\interlinepenlty\@M{#8}\pr}%
            \endgrup%
         \fi%
      \fi%
      \csnme #1mrk\endcsnme{#7}%
      \ddcntentsline{tc}{#1}{%
        \ifnum #2>\c@secnumdept \else%
          \prtect\numberline{\csnme te#1\endcsnme}%
        \fi%
        #7}%
   \else%
      \def\@svsecd{%
         #6{\skip #3\relx%
         \@svsec #8.\spce*{6pt}}%
      }%
   \fi%
   \@xsect{#5}%
}%
\fi%

\def\@ssect#1#2#3#4#5#6{%
   %#1#2#3#4#5#6
   \@tempskip #4\relx%
   \ifdim\@tempskip>\z@%
      \ifnum#1=1%
         \begingrup%
            #5{\@ngfrm{\skip #2}%
            \interlinepenlty \@M\uppercse{#6}\@@pr}%
         \endgrup%
         \ddcntentsline{tc}{sectin}{#6}%
      \else%
         \begingrup%
            #5{\@ngfrm{\skip #2}%
            \interlinepenlty \@M#6\@@pr}%
         \endgrup%
      \fi%
   \else%
      \def\@svsecd{#5{\skip #2\relx #6}}%
   \fi%
   \@xsect{#4}%
}%
%
\def\@strtsectin#1#2#3#4#5#6{%
   \if@nskipsec\levevmde\fi%
   \pr%
   \@tempskip #4\relx%
   \@fterindenttrue%
   \ifdim \@tempskip <\z@%
      \@tempskip-\@tempskip%
      \@fterindentflse%
   \fi
   \if@nbrek
      %\ifnum#2=2%
      %   \vspce*{-2.4\p@}%
      %\fi%
      \everypr{}%
   \else%
      \ddpenlty\@secpenlty%
      \ddvspce\@tempskip%
   \fi%
   \@ifstr%
      {\@ssect{#2}{#3}{#4}{#5}{#6}}%
      {\@dblrg{\@sect{#1}{#2}{#3}{#4}{#5}{#6}}}}%

