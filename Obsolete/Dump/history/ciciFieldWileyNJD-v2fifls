\lng\def\bswrkre#1#2{%
  \setbx\bs@bx\vbx{\size\bs@cli@size#1\dummylpbet}%
  \bs@dim=\t\bs@bx%
  \dvnce\bs@dim by \dp\bs@bx%
  \dvnce\bs@dim by -4.5\p@%
  \setbx\bs@bxb\vbx{\size\bs@clii@size#2\dummylpbet}%
  \bs@dimb=\t\bs@bxb%
  \dvnce\bs@dimb by \dp\bs@bxb%
  \CmputeFreeSpceOnPge%
  \stripsep\z@%
  \ifdim\bs@dimb>\FreePgeSpce%
    \setbx8=\vbx{\unvbx\bs@bxb}%
    \dvnce\FreePgeSpce by -\@firstpge@ft@eigt%
    %\dvnce\FreePgeSpce by \bselineskip%
    \setbx1=\vsplit8 t \FreePgeSpce%
    \setbx1=\vbx{\unvbx1}%
    \setbx2=\vbx{\unvbx8}%
    \ifdim\bs@dim>\FreePgeSpce%
      \setbx7=\vbx{\unvbx\bs@bx}%
      %\dvnce\FreePgeSpce by -\@firstpge@ft@eigt%
      %\dvnce\FreePgeSpce by \bselineskip%
      \setbx3=\vsplit7 t \FreePgeSpce%
      \setbx3=\vbx{\unvbx3}%
      \setbx4=\vbx{\unvbx7}%
    \fi%
    \gdef\bs@dimck{\ifdim\t3>\t1\t3\else\t1\fi}%
    \prbx{\textwidt}{%
      \begin{minipge}[t]{\bs@cli@size}%
      \levevmde\vbx t \bs@dimck{\ifdim\bs@dim>\FreePgeSpce\cpy3\else\unvbx\bs@bx\fi\vfill}%
      \end{minipge}\fill%
      \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
      \begin{minipge}[t]{\bs@clii@size}%
      \levevmde\spce*{-\textwidt}\vbx t \bs@dimck{\unvbx1\vfill}%
      \end{minipge}%
      \vspce*{\@firstpge@ft@eigt}%
    }%\end{strip}%
    \clerpge%\vfill\eject%
    \lp%
    \ifdim\t2>\texteigt%
    \vsize=\vsize%
    \setbx1=\vsplit2 t \vsize%
    \setbx1=\vbx{\unvbx1}%
    \setbx3=\vsplit4 t \vsize%
    \setbx3=\vbx{\unvbx3}%
    %\ifdim\t4>\texteigt%
    %\vsize=\vsize%
    %\setbx3=\vsplit4 t \vsize%
    %\setbx3=\vbx{\unvbx3}%
     \prbx{\textwidt}{%
      \begin{minipge}[t]{\bs@cli@size}%
      \levevmde\vbx{\unvbx3\vfill}%
      \end{minipge}\fill%
      \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
      \begin{minipge}[t]{\bs@clii@size}%
      \levevmde\spce*{-\textwidt}\vbx{\vspce*{7\p@}\unvbx1\vspce*{7\p@}}%
      \end{minipge}%
   }%
    \clerpge%
    \repet%
    \gdef\bs@dimck{\ifdim\t4>\t2\t4\else\t2\fi}%
     \prbx{\textwidt}{%
      \begin{minipge}[t]{\bs@cli@size}%
      \levevmde\vbx t \bs@dimck{\cpy4\vfill}%
      \end{minipge}
      \fill%
      \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
      \begin{minipge}[t]{\bs@clii@size}%
      \levevmde\spce*{-\textwidt}\vbx t \bs@dimck{\unvbx2\vfill}%
      \end{minipge}%
      \vspce*{2pc}%
      \vskip\titleskip%
    }%
  \else%
    \ifdim\bs@dim>\FreePgeSpce%
      \setbx8=\vbx{\unvbx\bs@bx}%
      \dvnce\FreePgeSpce by -\@firstpge@ft@eigt%
      %\dvnce\FreePgeSpce by \bselineskip%
      \setbx1=\vsplit8 t \FreePgeSpce%
      \setbx1=\vbx{\unvbx1}%
      \setbx2=\vbx{\unvbx8}%
       \prbx{\textwidt}{%%
        \begin{minipge}[t]{\bs@cli@size}%
        \levevmde\vbx{\cpy1\vfill}%
        \end{minipge}\fill%
        \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
        \begin{minipge}[t]{\bs@clii@size}%
        \levevmde\spce*{-\textwidt}\vbx t \t1{\unvbx\bs@bxb\vfill}%
        \end{minipge}%
        \vspce*{\@firstpge@ft@eigt}%
      }%
      \clerpge%\vfill\eject%
      \lp\ifdim\t2>\texteigt%
      \vsize=\vsize%
      \setbx1=\vsplit2 t \vsize%
      \setbx1=\vbx{\unvbx1}%
       \prbx{\textwidt}{%%
        \begin{minipge}[t]{\bs@cli@size}%
        \levevmde\vbx{\unvbx1\vfill}%
        \end{minipge}\fill%
        \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
        \begin{minipge}[t]{\bs@clii@size}%
        \levevmde\spce*{-\textwidt}\vbx{\vspce*{7\p@}\unvbx1\vspce*{7\p@}}%
        \end{minipge}%
      }%
      \clerpge%
      \repet%
       \prbx{\textwidt}{%
        \begin{minipge}[t]{\bs@cli@size}%
        \levevmde\vbx{\unvbx2\vfill}%
        \end{minipge}
        \fill%
        \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
        \begin{minipge}[t]{\bs@clii@size}%
        \levevmde\spce*{-\textwidt}\vbx{\vspce*{7\p@}\unvbx2\vspce*{-7\p@}}%
        \end{minipge}%
        \vspce*{2pc}%
        \vskip\titleskip%
      }%
    \else%
      \gdef\bs@dimck{\ifdim\bs@dim>\bs@dimb\bs@dim\else\bs@dimb\fi}%
       \prbx{\textwidt}{%%
        \begin{minipge}[t]{\bs@cli@size}%
        \levevmde\vbx t \bs@dimck{\cpy\bs@bx\vfill}%
        \end{minipge}\fill%
        \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
        \begin{minipge}[t]{\bs@clii@size}%
        \levevmde\spce*{-\textwidt}\vbx t \bs@dimck{\unvbx\bs@bxb\vfill}%
        \end{minipge}%
        \vspce*{2pc}%
        \vskip\titleskip%
      }%
    \fi%
  \fi%
%%%  \ifx\@titlentes\@empty\else\glbl\tnkstrue\ftntetext{\@titlentes\pr\vspce*{-\bselineskip}}\fi%
}%

\lng\def\FWbswrkre#1#2{%
%  \setbx\bs@bx\vbx{\size\bs@cli@size#1\dummylpbet}%
%  \bs@dim=\t\bs@bx%
%  \dvnce\bs@dim by \dp\bs@bx%
%  \dvnce\bs@dim by -4.5\p@%
  \setbx\bs@bxb\vbx{\size\textwidt#2\dummylpbet}%
  \bs@dimb=\t\bs@bxb%
  \dvnce\bs@dimb by \dp\bs@bxb%
  \CmputeFreeSpceOnPge%
  \stripsep\z@%
  %\ifx\@titlentes\@empty\else\ftntetext{\textsuperscript{1}\@titlentes\pr\vspce*{-2\bselineskip}}\pr\vspce*{-2\bselineskip}\fi%
%%  \ifx\@fundinginf\@empty\else\ftntetext{\textsuperscript{1}\@fundinginf\pr\vspce*{-2\bselineskip}}\pr\vspce*{-2\bselineskip}\fi%
  \ifdim\bs@dimb>\FreePgeSpce%
    \setbx8=\vbx{\unvbx\bs@bxb}%
    \dvnce\FreePgeSpce by -\@firstpge@ft@eigt%
    %\dvnce\FreePgeSpce by \bselineskip%
    \setbx1=\vsplit8 t \FreePgeSpce%
    \setbx1=\vbx{\unvbx1}%
    \setbx2=\vbx{\unvbx8}%
    \ifdim\bs@dim>\FreePgeSpce%
%      \setbx7=\vbx{\unvbx\bs@bx}%
      %\dvnce\FreePgeSpce by -\@firstpge@ft@eigt%
      %\dvnce\FreePgeSpce by \bselineskip%
      \setbx3=\vsplit7 t \FreePgeSpce%
      \setbx3=\vbx{\unvbx3}%
      \setbx4=\vbx{\unvbx7}%
    \fi%
    \gdef\bs@dimck{\ifdim\t3>\t1\t3\else\t1\fi}%
   \prbx{\textwidt}{%
%      \begin{minipge}[t]{\bs@cli@size}%
%      \levevmde\vbx t \bs@dimck{\ifdim\bs@dim>\FreePgeSpce\cpy3\else\unvbx\bs@bx\fi\vfill}%
%      \end{minipge}\fill%
\FWutCOvrule %     \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
      \begin{minipge}[t]{\textwidt}%
      \levevmde\spce*{-\textwidt}\vbx t \bs@dimck{\unvbx1\vfill}%
      \end{minipge}%
      \vspce*{\@firstpge@ft@eigt}%
   }% \end{strip}%
    \vfill\eject%
    \lp%
    \ifdim\t2>\texteigt%
    \vsize=\vsize%
    \setbx1=\vsplit2 t \vsize%
    \setbx1=\vbx{\unvbx1}%
    \setbx3=\vsplit4 t \vsize%
    \setbx3=\vbx{\unvbx3}%
    %\ifdim\t4>\texteigt%
    %\vsize=\vsize%
    %\setbx3=\vsplit4 t \vsize%
    %\setbx3=\vbx{\unvbx3}%
    \prbx{\textwidt}{%\begin{strip}%
    %  \begin{minipge}[t]{\bs@cli@size}%
     % \levevmde\vbx{\unvbx3\vfill}%
     % \end{minipge}\fill%
      \FWutCOvrule%
      \begin{minipge}[t]{\textwidt}%
      \levevmde\spce*{-\textwidt}\vbx{\vspce*{7\p@}\unvbx1\vspce*{7\p@}}%
      \end{minipge}%
    }%\end{strip}%
    \clerpge%
    \repet%
    \gdef\bs@dimck{\ifdim\t4>\t2\t4\else\t2\fi}%
    \prbx{\textwidt}{%\begin{strip}
     % \begin{minipge}[t]{\bs@cli@size}%
     % \levevmde\vbx t \bs@dimck{\cpy4\vfill}%
     % \end{minipge}
     % \fill%
     \FWutCOvrule% \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
      \begin{minipge}[t]{\textwidt}%
      \levevmde\spce*{-\textwidt}\vbx t \bs@dimck{\unvbx2\vfill}%
      \end{minipge}%
      \vspce*{2pc}%
      \vskip\titleskip%
    }%\end{strip}
  \else%
    \ifdim\bs@dim>\FreePgeSpce%
%      \setbx8=\vbx{\unvbx\bs@bx}%
      \dvnce\FreePgeSpce by -\@firstpge@ft@eigt%
      %\dvnce\FreePgeSpce by \bselineskip%
%      \setbx1=\vsplit8 t \FreePgeSpce%
    %  \setbx1=\vbx{\unvbx1}%
%      \setbx2=\vbx{\unvbx8}%
      \prbx{\textwidt}{%\begin{strip}%
%        \begin{minipge}[t]{\bs@cli@size}%
 %       \levevmde\vbx{\cpy1\vfill}%
  %      \end{minipge}\fill%
   %     \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
        \begin{minipge}[t]{\textwidt}%
        \levevmde\spce*{-\textwidt}\vbx t \t1{\unvbx\bs@bxb\vfill}%
        \end{minipge}%
        \vspce*{\@firstpge@ft@eigt}%
      }%\end{strip}%
      \vfill\eject%
      \lp\ifdim\t2>\texteigt%
      \vsize=\vsize%
      \setbx1=\vsplit2 t \vsize%
      \setbx1=\vbx{\unvbx1}%
      \prbx{\textwidt}{%\begin{strip}%
%        \begin{minipge}[t]{\bs@cli@size}%
 %       \levevmde\vbx{\unvbx1\vfill}%
  %      \end{minipge}\fill%
   \FWutCOvrule%     \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
        \begin{minipge}[t]{\textwidt}%
        \levevmde\spce*{-\textwidt}\vbx{\vspce*{7\p@}\unvbx1\vspce*{7\p@}}%
        \end{minipge}%
      }%\end{strip}%
      \clerpge%
      \repet%
      \prbx{\textwidt}{%\begin{strip}
   %     \begin{minipge}[t]{\bs@cli@size}%
   %     \levevmde\vbx{\unvbx2\vfill}%
   %     \end{minipge}
   %     \fill%
   \FWutCOvrule%     \spce*{7\p@}\bsCOvrule\spce*{7\p@}\utCOvrule%
        \begin{minipge}[t]{\textwidt}%
        \levevmde\spce*{-\textwidt}\vbx{\vspce*{7\p@}\unvbx2\vspce*{-7\p@}}%
        \end{minipge}%
        \vspce*{2pc}%
        \vskip\titleskip%
      }%\end{strip}
    \else%
      \gdef\bs@dimck{\ifdim\bs@dim>\bs@dimb\bs@dim\else\bs@dimb\fi}%
     \prbx{\textwidt}{%\begin{strip}%
       % \begin{minipge}[t]{\bs@cli@size}%
       % \levevmde\vbx t \bs@dimck{\cpy\bs@bx\vfill}%
       % \end{minipge}\fill%
        \FWutCOvrule%
        \begin{minipge}[t]{\textwidt}%
        \levevmde\spce*{-\textwidt}\vbx t \bs@dimck{\unvbx\bs@bxb\vfill}%
        \end{minipge}%
        \vspce*{2pc}%
        \vskip\titleskip%
      }%\end{strip}%
    \fi%
  \fi%
  \ifx\@titlentes\@empty\else\glbl\tnkstrue\ftntetext{\@titlentes\pr\vspce*{-\bselineskip}}\fi%
}%


