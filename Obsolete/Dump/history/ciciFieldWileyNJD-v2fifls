\gdef\printbstrctprt{%
  \bswrkre{%
    \nindent{\reset@fnt\ddressfnt\jmkddress\pr}%\mveduddress\@mveduddress%\@crres%
   % \nindent\ifx\mveduddress\@empty\else{\ddressfnt\@mveduddress\pr}\fi%
 \remvelstskip\vskip8pt%
   \ifx\@crresinf\empty\else{\crresinf\pr}\fi%
   \remvelstskip\vskip8pt%
   \ifx\@presentddress\empty\else\@presentddress\fi%
   \remvelstskip\vskip8pt%
    \ifx\@fundinginf\@empty\else\pr\@fundinginf\fi%
    \ifx\@JELinf\@empty\else\pr\@JELinf\fi%
  }{%
    \glbl\bstrcttrue%
    \ifx\@bstrct\@empty\else\@bstrct\fi%
\remvelstskip\vskip15pt%
    \ifx\@keywrds\@empty\else\@keywrds\fi%
    \glbl\bstrctflse%
  }
}

