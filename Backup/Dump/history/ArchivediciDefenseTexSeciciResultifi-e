\begin{frame}[fragile]
    \frametitle{Oblique shock relation -- $M_{\infty}(\hat{M})-\theta(\hat{\theta})-\beta_e(\hat{\beta}_e)-\beta_n(\hat{\beta}_n)$}
    \begin{columns}
        \begin{column}{.38\textwidth}
            \centering
            \includegraphics[width=\textwidth]{1_wedge_nomv_deg10_mach4_m1200_lined}
        \end{column}

        \begin{column}{.12\textwidth}
            \centering\tiny
            $M_{\infty}(4)-\theta(10^{\circ})-\beta_e(22.234^{\circ})-\beta_n(22.227^{\circ})$
        \end{column}

        \begin{column}{.38\textwidth}
            \centering
            \includegraphics[width=\textwidth]{1_wedge_nomv_deg15_mach6_m1200_lined}
        \end{column}

