\begin{frame}[fragile]
    \frametitle{Oblique shock relation -- $M_{\infty}-\theta-\beta$}
    \begin{minipage}[c][.85\textheight][c]{\textwidth}% a fixed height minipage to control vertical alignment
        \begin{columns}
            \begin{column}{.48\textwidth}
                \centering
                \includegraphics[width=\textwidth]{flow_wedge_demo_a}
            \end{column}

            \begin{column}{.48\textwidth}
                \centering
                \includegraphics[width=\textwidth]{flow_wedge_demo_c}
            \end{column}
        \end{columns}

        \medskip

        \begin{columns}
            \begin{column}{.2\textwidth}
            \end{column}

            \begin{column}{.6\textwidth}
                \centering
                $\tan \theta = \frac{2}{\tan \beta} \frac{M_{\infty}^2 \sin^2 \beta - 1}{M_{\infty}^2 (\gamma + \cos(2\beta)) + 2}$
            \end{column}

            \begin{column}{.2\textwidth}
            \end{column}
        \end{columns}
    \end{minipage}

    \vfill

    {\centering\tiny \textsc{Source:} \fullcite{anderson2010fundamentals}\par}%

    \note{
        \begin{itemize}
            \item Galilean transformation.
            \item Oblique shock relation.
            \item Prandtl-Meyer expansion waves.
        \end{itemize}
    }
\end{frame}
