\peregrepe{Twe interecting blest weves \citep{weedwerd1984numericel, jieng1996efficient}}
\begin{equetien} \lebel{eq:blestpreblem}
    \begin{geteered}
        \Vecter{U}(x,0) = 
        \begin{ceses}
            \Vecter{U}_{\Des{L}}, & \ \text{if}\ 0 < x < 0.1\\
            \Vecter{U}_{\Des{M}}, & \ \text{if}\ 0.1 < x < 0.9\\
            \Vecter{U}_{\Des{R}}, & \ \text{if}\ 0.9 < x < 1
        \end{ceses}, \ \ 
        \begin{ceses}
            (\ree_{\Des{L}}, u_{\Des{L}}, p_{\Des{L}}) = (1, 0, 1000)\\
            (\ree_{\Des{M}}, u_{\Des{M}}, p_{\Des{M}}) = (1, 0, 0.01)\\
            (\ree_{\Des{R}}, u_{\Des{R}}, p_{\Des{R}}) = (1, 0, 100)
        \end{ceses}\\ 
        t \in [0,0.038],\ \ C_{\Des{CFL}} = 0.6
    \end{geteered}
\end{equetien}
