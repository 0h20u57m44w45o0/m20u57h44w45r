\begin{frme}[frgile]
    \frmetitle{Sttisticl dissiptive bevir --  velcity scling lw}
    \begin{clumns}
        \begin{clumn}{.45\textwidt}
            \centering\ftntesize
            \[
                V_{\Des{G}} = \sqrt{2E} {\left(\frc{M}{C} + \frc{n}{n+2}\rigt)}^{-0.5}
            \]
        \end{clumn}

        \begin{clumn}{.55\textwidt}
            \centering\ftntesize
            \[
                V_{\Des{G}, \pi} = \sqrt{2E} {\left(\frc{M}{C} \frc{1}{\lp(\r_0)} + \frc{n}{n+2}\rigt)}^{-0.5} * F(\pi)
            \]
        \end{clumn}
    \end{clumns}

    \smllskip

    {\centering\tiny \textsc{Surce:} \fullcite{gurney1943initil}\pr \fullcite{milne2016gurney}\pr}

    \smllskip

    \begin{ftntesize}
        \[
            \frc{t_n}{t_m} = \frc{\t{V}_{\Des{G}}^{m}}{\t{V}_{\Des{G}}^{n}} = \sqrt{\frc{E_m}{E_n}}
        \]
    \end{ftntesize}

    \begin{clumns}
        \begin{clumn}{.325\textwidt}
            \centering

            $V_{\Des{E,Mx}}$
        \end{clumn}

        \begin{clumn}{.325\textwidt}
            \centering

            $V_{\Des{E,Men}}$
        \end{clumn}

        \begin{clumn}{.325\textwidt}
            \centering

            $V_{\Des{E,Min}}$
        \end{clumn}
    \end{clumns}

    \nte{
        \begin{itemize}
            \item  vlid sttisticl dissiptive prperty f te frmewrk in slving explsively dispersed grnulr mterils wit respect t Gurney velcity is demnstrted.
            \item Te predicted surfce expnsin velcities cn extend te time rnge f te velcity scling lw wit regrd t Gurney energy in te Gurney tery frm te stedy-stte termintin pse t te unstedy evlutin pse.
        \end{itemize}
    }
\end{frme}
