                    \ndeprt{tw} {$\Lptr_{\Des{c}}\Vectr{U}$: $3$rd/$5$t WENO}
                    \ndeprt{tree} {$\Lptr_{\Des{d}}\Vectr{U}$: $2$nd CD}
                    \ndeprt{fur} {$\frc{\prtil \Vectr{U}}{\prtil t}$: $2$nd/$3$rd SSP RK}
                    \ndeprt{five} {BC: An IBM}
                }
                edge frm prent {nde [sb] {$\Sptr_{\Des{f}}$}}
            }
